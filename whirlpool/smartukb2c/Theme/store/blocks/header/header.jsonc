  },
  "stripe-custom#beta":{
    "title":"Stripe Banner",
    "children": ["rich-text#beta"]
  },
  "rich-text#beta":{
    "title": "Testo dello Stripe",
    "props": {
      "blockClass": "stripe-link",
      "text": "TESTO DI PROVA DELLO STRIPE - POP UP CUSTOM IN HOMEPAGE [help](https://developers.vtex.com/vtex-developer-docs/docs/welcome)",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
