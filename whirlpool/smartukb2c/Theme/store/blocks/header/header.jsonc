    "props": {
      "id": "menu-item-sign-in",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "blockClass": "top-menu",
      "itemProps": {
        "type": "internal",
        "href": "/login?returnUrl=%2Faccount&",
        "noFollow": false
