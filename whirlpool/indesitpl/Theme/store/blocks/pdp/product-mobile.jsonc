  //       "type": "show"
  //     }
  //   }
  // },
  /* ---------------------- FEATURES COL ---------------------------- */
  "flex-layout.col#features-pdp-mobile": {
    "props": {
      "width": "100%",
      "blockClass": "features-col-style"
    },
    "children": ["flex-layout.row#features-container"]
  },
  /* ---------------------- BUTTON LAYER 4 ---------------------------- */
  "flex-layout.row#button-layer-4-mobile": {
    "props": {
      "blockClass": ""
    },
    "children": [
      /* "reevoo-badge-mobile" */
    ]
  },
  "reevoo-badge-mobile": {},
  /* ---------------------- PRODUCT MEASURES MOBILE ----------------------------  */
  "flex-layout.row#measures-mobile": {
    "props": {},
    "children": ["flex-layout.col#measure-mobile"]
  },
  "flex-layout.col#measure-mobile": {
    "props": {
      "blockClass": "measure-col-style-mobile"
    },
    "children": [
      "rich-text#measures-text-mobile",
      "product-specification-group#minimal"
    ]
  },
  "rich-text#measures-text-mobile": {
    "props": {
      "text": "Dimensions",
      "textPosition": "Left",
      "blockClass": "measure-text-style-mobile"
    }
  },
  /* -------------------------- CUSTOMER REVIEWS ---------------------------*/
  "rich-text#costumer-reviews-mobile": {
    "props": {
      "text": "Avis clients",
      "blockClass": "costumer-reviews-text-style-mobile"
    }
  },
  /* ---------------------- MODAL REVIEW ----------------------------  */
  "flex-layout.row#modal-review-mobile": {
    "children": ["modal-review#pdp-mobile"]
  },
  "modal-review#pdp-mobile": {},
  /* ---------------------- PRODUCT REVIEW ----------------------------  */
  "flex-layout.row#product-review-container-mobile": {
    "props": {
      "blockClass": "product-review-container-style"
    },
    "children": ["product-review"]
  }
