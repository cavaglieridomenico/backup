{
  "store.custom#pack-cuisine": {
    "blocks": [
      "toggle-layout#breadcrumb",
      "rich-text#title-pack-cuisine",
      // "flex-layout.row#pack-main-banner",
      "flex-layout.row#halfSpace",
      "toggle-layout#halfBannerHomePack"
      // "analytics"
    ]
  },
  "toggle-layout#breadcrumb": {
    "props": {
      "visible": false
    },
    "children": ["breadcrumb#pack"]
  },
  "breadcrumb#pack": {
    "props": {
      "categories": ["kolekcje"],
      "showOnMobile": false
    }
  },
  "flex-layout.row#pack-main-banner": {
    "props": {
      "blockClass": ["promoBanner", "reverse"]
    },
    "children": ["flex-layout.col#text-pack", "flex-layout.col#image-pack"]
  },
  "flex-layout.col#bannerFooterContainer": {
    "props": {},
    "children": ["rich-text#packBannerFooter1", "rich-text#packBannerFooter2"]
  },
  "flex-layout.col#image-pack": {
    "children": ["image#packCardImage"]
  },
  "image#packCardImage": {
    "props": {
      "src": "assets/placeholders/placeholder-image-square.jpg",
      "blockClass": "promoBanner"
    }
  },
  "flex-layout.col#text-pack": {
    "props": {
      "blockClass": ["promoBanner", "bannerPackPromo"]
    },
    "children": ["rich-text#titlepack", "flex-layout.col#bannerFooterContainer"]
  },
  "rich-text#titlepack": {
    "props": {
      "blockClass": "titleCard",
      "textAlignment": "left",
      "text": "Sélectionnez les appareils électroménager de votre pack et bénéficiez de remises* à partir de 2 produits**"
    }
  },
  "rich-text#packBannerFooter1": {
    "props": {
      "blockClass": ["postilla"],
      "textAlignment": "left",
      "text": "*Mention légale: la remise s’applique sur les produits encastrables (four, plaque, hotte, micro-ondes, lave-vaisselle) et dans la limite d’un produit par catégorie."
    }
  },
  "rich-text#packBannerFooter2": {
    "props": {
      "blockClass": ["postilla", "biggerTextPostilla"],
      "textAlignment": "left",
      "text": "**Réduction de -10% sur votre commande à partir de 2 produits commandés."
    }
  },
  "rich-text#title-pack-cuisine": {
    "props": {
      "text": "Équipez ou renouveler intégralement votre cuisine!",
      "textAlignment": "center",
      "blockClass": ["textHome"],
      "textPosition": "CENTER"
    }
  },
  "toggle-layout#halfBannerHomePack": {
    "props": {
      "visible": true
    },
    "children": [
      "info-card#halfPack1",
      "info-card#halfPack2",
      "info-card#halfPack3",
      "info-card#halfPack4",
      "info-card#halfPack5",
      "toggle-layout#halfBannerPack"
    ]
  },
  "info-card#halfPack1": {
    "props": {
      "id": "half-banner-seconf",
      "isFullModeStyle": false,
      "imageUrl": "assets/placeholders/placeholder-image-square.jpg",
      "headline": "Everest Serie400",
      "subhead": "La più grande rivoluzione nel mondo del built-in.",
      "callToActionText": "En savoir plus",
      "callToActionUrl": "#",
      "blockClass": "halfBannerReverse",
      "textAlignment": "left"
    }
  },
  "info-card#halfPack3": {
    "props": {
      "id": "half-banner-seconf",
      "isFullModeStyle": false,
      "imageUrl": "assets/placeholders/placeholder-image-square.jpg",
