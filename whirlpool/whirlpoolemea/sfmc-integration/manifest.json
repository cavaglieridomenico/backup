              "generalInfo": {
                "title": "General info",
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "serviceIds": {
                      "title": "Service ids (comma separated)",
                      "type": "string"
                    },
                    "serviceName": {
                      "title": "Service name",
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "key": {
                            "title": "Locale",
                            "type": "string"
                          },
                          "value": {
                            "title": "Value",
                            "type": "string"
                          }
                        }
                      }
                    }
                  }
                }
              },
              "infoPerSalesChannel": {
                "title": "Specific info per trade policy",
                "type": "array",
                "items": {
                  "type": "object",
                  "properties": {
                    "salesChannelId": {
                      "title": "Sales channel id",
                      "type": "string"
                    },
                    "serviceIds": {
                      "title": "List of service ids (comma separated) per sales channel",
                      "type": "string"
