                    "installation": {
                      "title": "Id of the service \"Installation\"",
                      "type": "string"
                    },
                    "fiveYearsWarranty": {
                      "title": "Id of the service \"5 years warranty\"",
                      "type": "string"
                    },
                    "legalWarranty": {
                      "title": "Id of the service \"Legal warranty\"",
                      "type": "string"
                    },
                    "extendedWarranty": {
                      "title": "Id of the service \"Carefree plus\"",
                      "type": "string"
                    }
                  }
                }
              },
              "serviceInTotals": {
                "title": "Send services in the order totals (order confirmation)",
                "type": "boolean",
                "default": false
              }
            }
          },
          "specifications": {
            "title": "Specifications",
