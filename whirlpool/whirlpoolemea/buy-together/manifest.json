{
  "vendor": "whirlpoolemea",
  "name": "buy-together",
  "version": "0.0.6",
  "title": "Buy together component",
  "description": "Component to show bundles",
  "defaultLocale": "en-EN",
  "builders": {
    "messages": "1.x",
    "react": "3.x",
    "store": "0.x",
    "styles": "2.x",
    "graphql": "1.x",
    "node": "6.x",
    "docs": "0.x"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "dependencies": {
    "vtex.search-resolver": "1.x",
    "vtex.add-to-cart-button": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.device-detector": "0.x",
    "vtex.flex-layout": "0.x",
    "vtex.format-currency": "0.x",
    "vtex.messages": "1.x",
    "vtex.pixel-manager": "1.x",
    "vtex.product-context": "0.x",
    "vtex.product-group-context": "0.x",
    "vtex.product-list-context": "0.x",
    "vtex.product-price": "1.x",
    "vtex.product-summary": "2.x",
    "vtex.responsive-layout": "0.x",
    "vtex.store-components": "3.x",
    "vtex.styleguide": "9.x",
    "vtex.slider-layout": "0.x",
    "vtex.store-resources": "0.x",
    "vtex.search-graphql": "0.x",
    "vtex.recommendation-context": "0.x"
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "vtex.search-resolver:resolve-graphql"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    }
  ],
  "settingsSchema": {
    "title": "Buy together component",
    "type": "object",
    "bindingBounded": true,
    "properties": {
      "salesChannel": {
        "type": "string",
        "title": "Sales channel ID",
        "default": "1",
        "description": "Sales channel for obtaining product information"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
