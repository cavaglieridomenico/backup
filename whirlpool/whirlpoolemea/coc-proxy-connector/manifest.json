{
  "name": "coc-proxy-connector",
  "vendor": "whirlpoolemea",
  "version": "0.0.3",
  "title": "CoC Proxy Connector",
  "description": "Reference app for the proxy connector between CoC and WorldPay",
  "mustUpdateAt": "2018-01-04",
  "categories": [],
  "dependencies": {},
  "builders": {
    "node": "6.x",
    "docs": "0.x"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "secure-test.worldpay.com",
        "path": "/jsp/merchant/xml/paymentService.jsp"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "secure.worldpay.com",
        "path": "/jsp/merchant/xml/paymentService.jsp"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "*"
      }
    }
  ],
  "settingsSchema": {
    "title": "Proxy Settings",
    "type": "object",
    "properties": {
      "basicAuthUsername": {
        "title": "Basic Auth Username for incoming API call",
        "type": "string"
      },
      "basicAuthPassword": {
        "title": "Basic Auth Password for incoming API call",
        "type": "string"
      },
      "wpUrl": {
        "title": "API Url Worldpay",
        "type": "string",
        "default": "https://secure-test.worldpay.com"
      },
      "wpusername": {
        "title": "Username Worldpay",
        "type": "string"
      },
      "wppassword": {
        "title": "Password Worldpay",
        "type": "string"
      }
    }
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}