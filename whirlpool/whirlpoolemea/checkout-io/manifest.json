{
  "vendor": "whirlpoolemea",
  "name": "checkout-container-custom",
  "version": "2.0.56",
  "title": "Custom Checkout IO",
  "description": "",
  "builders": {
    "assets": "0.x",
    "react": "3.x",
    "store": "0.x",
    "messages": "1.x",
    "node": "6.x",
    "graphql": "1.x",
    "styles": "2.x"
  },
  "dependencies": {
    "vtex.store-graphql": "2.x",
    "vtex.pixel-interfaces": "1.x",
    "vtex.pixel-manager": "1.x",
    "vtex.styleguide": "9.x",
    "vtex.formatted-price": "0.x",
    "vtex.css-handles": "0.x",
    "vtex.shipping-estimate-translator": "2.x",
    "vtex.flex-layout": "0.x",
    "vtex.rich-text": "0.x",
    "vtex.store-icons": "0.x",
    "vtex.store-components": "3.x",
    "vtex.store-footer": "2.x",
    "vtex.store-header": "2.x",
    "vtex.login": "2.x",
    "vtex.responsive-layout": "0.x",
    "vtex.store": "2.x",
    "vtex.checkout-cart": "0.x",
    "vtex.order-placed": "2.x",
    "vtex.order-manager": "0.x",
    "vtex.checkout-graphql": "0.x",
    "vtex.search-graphql": "0.x",
    "vtex.checkout-resources": "0.x",
    "vtex.render-runtime": "8.x",
    "vtex.apps-graphql": "3.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*",
        "path": "*"
      }
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "AcessaTodosCarrinhos"
    },
    {
      "name": "AcessaTodosPedidos"
    },
    {
      "name": "OMSViewer"
    }
  ],
  "settingsSchema": {
    "title": "Checkout IO",
    "type": "object",
    "access": "public",
    "properties": {
      "googleMapsApiKey": {
        "title": "Google Maps Api Key",
        "description": "Enter the google maps api key used for autocomplete inside checkout io",
        "type": "string",
        "default": ""
      },
      "country": {
        "title": "Country Code",
        "description": "Enter the 2-digit code of the country (e.g. GB)",
        "type": "string",
        "default": "IT"
      },
      "defaultCurrencyCode": {
        "title": "Default Currency Code",
        "description": "Enter the default currency code (e.g. GBP)",
        "type": "string",
        "default": "GBP"
      },
      "defaultLocale": {
        "title": "Default Locale",
        "description": "Enter the default locale (e.g. en-GB)",
        "type": "string",
        "default": "en-GB"
      },
      "isMultilanguage": {
        "title": "Is Multilanguage",
        "description": "Define if the website must support the multilanguage",
        "type": "boolean",
        "default": false
      },
      "enableLogs": {
        "title": "Enable Logs",
        "description": "Define if the logs must be enabled",
        "type": "boolean",
        "default": false
      },
      "paypalClientId": {
        "title": "PayPal Client ID",
        "description": "Enter the client id for configuring the PayPal script",
        "type": "string",
        "default": ""
      },
      "paypalConnectorId": {
        "title": "PayPal Connector ID",
        "description": "Enter the id of the PayPal connector from the payment settings (e.g. 12)",
        "type": "string",
        "default": "12"
      },
      "worldpayConnectorId": {
        "title": "WorldPay ID",
        "description": "Enter the id of the WorldPay connector from the payment settings (e.g. 202 or 201)",
        "type": "string",
        "default": "201"
      },
      "adyenConnectorId": {
        "title": "Adyen ID",
        "description": "Enter the id of the Adyen pay connector from the payment settings (e.g. 2)",
        "type": "string",
        "default": "2"
      },
      "googlePayConnectorId": {
        "title": "Google Pay ID",
        "description": "Enter the id of the Google Pay connector from the payment settings (e.g. 69)",
        "type": "string",
        "default": "169"
      },
      "applePayConnectorId": {
        "title": "Apple pay ID",
        "description": "Enter the id of the Apple pay connector from the payment settings (e.g. 69)",
        "type": "string",
        "default": "69"
      },
      "isCC": {
        "title": "Is CC",
        "description": "Define if the website is a Closed Community",
        "type": "boolean",
        "default": false
      },
      "ccProperties": {
        "title": "CC Properties",
        "type": "object",
        "properties": {
          "loginReturnUrl": {
            "title": "Login Return URL",
            "description": "Enter the URL to redirect the user after the login in the checkout",
            "type": "string",
            "default": "/checkout/#/profile"
          }
        }
      },
      "multiseller": {
        "title": "Multi-Seller Marketplace",
        "type": "object",
        "properties": {
          "isEnabled": {
            "title": "Enable",
            "type": "boolean",
            "default": false
          },
          "sellers": {
            "title": "Sellers",
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "categoryId": {
                  "title": "Category id",
                  "type": "string"
                },
                "sellerList": {
                  "title": "Seller list (comma separated)",
                  "type": "string"
                }
              }
            }
          }
        }
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
