{
  "name": "one-trust",
  "vendor": "whirlpoolemea",
  "version": "5.0.1",
  "title": "OneTrust",
  "description": "One trust integration",
  "builders": {
    "store": "0.x",
    "pixel": "0.x",
    "docs": "0.x"
  },
  "dependencies": {
    "vtex.pixel-interfaces": "1.x"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "settingsSchema": {
    "title": "One Trust",
    "type": "object",
    "properties": {
      "cdnId": {
        "title": "CDN ID",
        "description": "Enter the ID cdn ID from your One Trust consolle",
        "type": "string"
      },
      "service": {
        "title": "Service",
        "description": "Enter the service URL for One Trust",
        "type": "string"
      },
      "url": {
        "title": "Url",
        "description": "Enter the URL for One Trust",
        "type": "string"
      },
      "domain": {
        "title": "Data Domain",
        "description": "Data Domain",
        "type": "string"
      },
      "consent": {
        "title": "Text Button",
        "description": "Text on accepting cookie button",
        "type": "string"
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
