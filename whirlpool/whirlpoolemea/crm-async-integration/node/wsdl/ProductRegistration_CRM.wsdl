     <xsd:element name="Contactallowance" type="n0:char1"/>  <!-- Always value 3 should be -->
     <xsd:element name="Partnerexternal" type="n0:char20"/> <!-- Leave it blank -->
     <xsd:element name="Dataorigintype" type="n0:char4"/> <!--  Always value OREG should be -->
    </xsd:sequence>
   </xsd:complexType>
