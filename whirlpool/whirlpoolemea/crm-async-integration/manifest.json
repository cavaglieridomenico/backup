  "settingsSchema": {
    "title": "CRM connector",
    "type": "object",
    "properties": {
      "crmEnvironment": {
        "title": "CRM - Environment",
        "type": "string",
        "enum": [
          "production",
          "quality"
        ],
        "default": "quality"
      },
      "crmPassword": {
        "title": "CRM - Password",
        "type": "string"
      },
      "useSapPo": {
        "title": "SAP PO",
        "type": "boolean"
      },
      "isUkProject": {
        "title": "UK integration",
        "type": "boolean"
      },
      "doubleOptin": {
        "title": "Double Optin",
        "type": "boolean"
      },
      "profilingOptin": {
        "title": "Profiling Optin",
        "type": "boolean"
      },
      "checkoutAsGuest": {
        "title": "Checkout as Guest",
        "type": "boolean"
      },
      "newsletterAsGuest": {
        "title": "Newsletter as Guest",
        "type": "boolean"
      },
      "newsletterMDEntity": {
        "title": "Newsletter - MD Entity",
        "type": "string",
        "default": "NL"
      },
      "attrOptin": {
        "title": "CRM - Optin attributes (comma separated)",
        "type": "string"
      },
      "attrChannel": {
        "title": "CRM - Channel attributes (comma separated)",
        "type": "string"
      },
      "attrProfiling": {
        "title": "CRM - Profiling attributes (comma separated)",
        "type": "string"
      },
      "attrSourceCampaign": {
        "title": "CRM - Source campaign attributes (comma separated)",
        "type": "string"
      },
      "allowedStates": {
        "title": "CRM - Allowed states (comma separated)",
        "type": "string"
      },
      "defaultLocale": {
        "title": "CRM - Default locale (2 chars, example: EN)",
        "type": "string"
      },
      "defaultSource": {
        "title": "CRM - Default source (MAX 4 chars, example: MYAC)",
        "type": "string"
      },
      "defaultCountry": {
        "title": "CRM - Default country (2 chars, example: GB)",
        "type": "string"
      },
      "webIdPrefix": {
        "title": "CRM - WebId: prefix (e.g.: CCUK, BAUDE)",
        "type": "string"
      },
      "maxNumOfCharForWebId": {
        "title": "CRM - Web Id: maximum length",
        "type": "number"
      },
      "maxNumOfDigitsForPhone": {
        "title": "CRM - Phone number: maximum length",
        "type": "number"
      },
      "crmRecoverPlan": {
        "title": "CRM - Recover plan",
        "type": "boolean"
      },
      "crmRecoverPlanStartDate": {
        "title": "CRM - Recover plan - Start date (yyyy-mm-dd)",
        "type": "string"
      },
      "newsletterAsGuestThroughGCP": {
        "title": "Newsletter - GCP integration",
        "type": "boolean"
      },
      "gcpHost": {
        "title": "GCP - Host",
        "type": "string"
      },
      "gcpProjectId": {
        "title": "GCP - Project id",
        "type": "string"
      },
      "gcpClientEmail": {
        "title": "GCP - Client email",
        "type": "string"
      },
      "gcpPrivateKey": {
        "title": "GCP - Private key",
        "type": "string"
      },
      "gcpTargetAudience": {
        "title": "GCP - Target audience",
        "type": "string"
      },
      "gcpBrand": {
