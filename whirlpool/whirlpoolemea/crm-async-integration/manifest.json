          "whiteList": {
            "title": "MD - White list",
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "keyFields": {
            "title": "SF - EPP Import - Key Fields",
            "type": "string",
            "enum": [
              "email",
              "clockNumber, hrNumber"
            ],
            "default": "clockNumber, hrNumber"
          },
          "tradePolicyId": {
            "title": "Trade policy id",
            "type": "string"
          },
          "loginUrl": {
            "title": "Login URL",
            "type": "string"
          },
          "attrOptinCC": {
