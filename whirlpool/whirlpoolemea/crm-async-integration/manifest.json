        "type": "string"
      },
      "gcpCountry": {
        "title": "GCP - Country",
        "type": "string"
      },
      "crmEntityName": {
        "title": "Vtex - CRM Backup: MD entity name",
        "type": "string",
        "default": "CC"
      },
      "enabledAPICredentials": {
        "title": "Vtex - Enabled API credentials",
        "type": "string"
      },
      "MDKey": {
        "title": "Vtex - MD key",
        "type": "string"
      },
      "enabledMDKeyHash": {
        "title": "Vtex - Enabled MD key (hash)",
        "type": "string"
      },
      "localTimeLocale": {
        "title": "Vtex - Locale time - Locale (5 chars, example: en-GB)",
        "type": "string"
      },
      "localTimeZone": {
        "title": "Vtex - Locale time - Zone (example: Europe/Rome)",
        "type": "string"
      },
      "authCookie": {
        "title": "Vtex - Authentication cookie",
        "type": "string",
        "default": "VtexIdclientAutCookie_{{account}}"
      },
      "pixelCrmBackflow": {
        "title": "Vtex - Enable pixel for CRM backflow",
        "type": "boolean"
      },
      "isCCProject": {
        "title": "CC Project",
        "type": "boolean"
      },
      "epp": {
        "title": "EPP",
        "type": "object",
