{
  "name": "order-tracker",
  "vendor": "whirlpoolemea",
  "version": "0.0.1",
  "title": "Order Tracker",
  "description": "Block used to retrieve informations about last order status",
  "mustUpdateAt": "2018-01-04",
  "categories": [],
  "dependencies": {
    "vtex.css-handles": "0.x",
    "vtex.product-context": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x",
    "graphql": "1.x",
    "messages": "1.x",
    "store": "0.x",
    "react": "3.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "httpstat.us",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.myvtex.com",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "ADMIN_DS"
    }
  ],
  "billingOptions": {
    "type": "free",
    "availableCountries": [
      "*"
    ]
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
