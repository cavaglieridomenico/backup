{
  "memory": 256,
  "ttl": 10,
  "timeout": 60,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "events": {
    "newOrder": {
      "sender": "vtex.orders-broadcast",
      "topics": [
        "payment-approved",
        "window-to-cancel",
        "ready-for-handling",
        "handling",
        "invoiced"
      ]
    }
  },
  "routes": {
    "cartChecker": {
      "path": "/app/cartlimiter/orderform/:orderFormId/check",
      "public": true
    },
    "updateCartItems": {
      "path": "/app/cartlimiter/orderform/:orderFormId/items/update",
      "public": true
    },
    "countOrder": {
      "path": "/app/cartlimiter/order/:orderId/counter",
      "public": true
    }
  }
}
