{
  "name": "worldpay-connector",
  "vendor": "whirlpoolemea",
  "version": "1.0.5",
  "title": "Worldpay",
  "description": "Worldpay connector",
  "mustUpdateAt": "2021-03-23",
  "categories": [],
  "dependencies": {},
  "builders": {
    "node": "6.x",
    "docs": "0.x"
  },
  "billingOptions": {
    "termsURL": "https://compliance.vtex.com/gdpr/policies/vtex-privacy-policy",
    "support": {
      "url": "https://support.vtex.com/hc/requests"
    },
    "free": true
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "secure-test.worldpay.com",
        "path": "/jsp/merchant/xml/paymentService.jsp"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*.myvtex.com",
        "path": "/_v/api/connectors/worldpay/notification"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "secure.worldpay.com",
        "path": "/jsp/merchant/xml/paymentService.jsp"
      }
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexpayments.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "heimdall.vtexpayments.com.br",
        "path": "/api/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/oms/pvt/orders/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "cert-xipayapi.paymetric.com",
        "path": "/PMXIGGE/XiPay30WS.asmx"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "xipayapi.paymetric.com",
        "path": "/PMXIGGE/XiPay30WS.asmx"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vbase-read-write"
    }
  ],
  "settingsSchema": {
    "title": "Worldpay",
    "type": "object",
    "properties": {
      "vtexAppKey": {
        "title": "VTEX app key",
        "type": "string",
        "default": "vtex_app_key"
      },
      "vtexAppToken": {
        "title": "VTEX app token (sha-256)",
        "type": "string",
        "default": "74d0b37dfe7c398a8ecd8b73b2f0d5691afc4449cd08157b29729edfa05024d5"
      },
      "wpUrl": {
        "title": "API Url Worldpay",
        "type": "string",
        "default": "https://secure-test.worldpay.com"
      },
      "merchantCode": {
        "title": "Merchant Code Worldpay",
        "type": "string",
        "default": "VTEXIT"
      },
      "installationID": {
        "title": "Installation ID Worldpay",
        "type": "string",
        "default": "1421215"
      },
      "wpusername": {
        "title": "Username Worldpay",
        "type": "string",
        "default": "YGUSMCL13L6VKKJNF3PU"
      },
      "wppassword": {
        "title": "Password Worldpay",
        "type": "string",
        "default": "0d4e757e32e75807b47ece34212828a6939271249812726cfbdc7f41dd0b7d9f"
      },
      "paymentAppName": {
        "title": "Payment App Name",
        "type": "string",
        "default": "itwhirlpoolqa.worldpay-payment-app"
      },
      "locale": {
        "title": "Worldpay iFrame language",
        "type": "string",
        "default": "it-it"
      },
      "wpVersion": {
        "title": "Worldpay API version",
        "type": "string",
        "default": "1.4"
      },
      "defaultCurrencyCode": {
        "title": "Default Currency Code",
        "type": "string"
      },
      "gcpappkey": {
        "title": "GCP app key",
        "type": "string",
        "default": "vtexappkey-itwhirlpoolqa-SEMTZI"
      },
      "gcpapptoken": {
        "title": "GCP app token (sha-256)",
        "type": "string",
        "default": "8de1cbc3900be9c822bc80486cb391ce0851733884fd29a5f0aae537af887d6a"
      },
      "useXipay": {
        "title": "Xipay enabled",
        "type": "boolean",
        "default": true
      },
      "xipayUrl": {
        "title": "XiPay URL",
        "type": "string",
        "default": "https://cert-xipayapi.paymetric.com"
      },
      "xipayUsername": {
        "title": "XiPay Username",
        "type": "string",
        "default": "Paymetric\\WSAWhirlpool_EUQA"
      },
      "xipayPassword": {
        "title": "XiPay Password",
        "type": "string",
        "default": "1726c478a887cb073dd31ec2243b3bf2"
      },
      "xipayMerchantid": {
        "title": "XiPay Merchant ID",
        "type": "string",
        "default": "VTEXIT"
      },
      "sapmid": {
        "title": "SAP Merchant ID",
        "type": "string",
        "default": "VTEXIT"
      },
      "sapgl": {
        "title": "SAP GL Account",
        "type": "string",
        "default": "1330030339"
      },
      "p24DelayToCancel": {
        "title": "P24 transaction time to cancel in seconds",
        "type": "number",
        "default": 3000
      },
      "countryCodeMapping": {
        "title": "Country Code Mapping",
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "title": "Country Name",
              "type": "string"
            },
            "code": {
              "title": "Country Code",
              "type": "string"
            }
          }
        },
        "default": [
          {
            "name": "italy",
            "code": "IT"
          },
          {
            "name": "italia",
            "code": "IT"
          },
          {
            "name": "ita",
            "code": "IT"
          },
          {
            "name": "san marino",
            "code": "SM"
          },
          {
            "name": "smr",
            "code": "SM"
          },
          {
            "name": "vaticano",
            "code": "VA"
          },
          {
            "name": "vat",
            "code": "VA"
          },
          {
            "name": "france",
            "code": "FR"
          },
          {
            "name": "poland",
            "code": "PL"
          },
          {
            "name": "pol",
            "code": "PL"
          }
        ]
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}