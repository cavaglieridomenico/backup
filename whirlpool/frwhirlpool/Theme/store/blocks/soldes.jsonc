    "props": {
      "blockClass": "containerBlackFriday2022"
    }
  },
  "image#whirlpool-logo-soldes": {
    "props": {
      "src": "assets/whl-logo-white.png",
      "blockClass": "whirlpoolLogo"
    }
  },
  "rich-text#soldesHeading": {
    "props": {
      "text": "# BLACK \n FRIDAY",
      "blockClass": "blackFriday2022"
    }
