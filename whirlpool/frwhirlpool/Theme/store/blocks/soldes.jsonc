      "flex-layout.row#card1Soldes"
    ]
  },
  "toggle-layout#card4Soldes": {
    "children": [
      "flex-layout.row#card1Soldes"
    ]
  },
  "flex-layout.row#card1Soldes": {
    "props": {
      "fullWidth": true
    },
    "children": [
      "info-card#Grid1Soldes"
    ]
  },
  "info-card#Grid1Soldes": {
    "props": {
      "isFullModeStyle": false,
      "imageUrl": "assets/grid1.jpg",
      "headline": "Fours Absolute",
      "subhead": "Cuisinez facilement et de manière interactive",
      "callToActionText": "Découvrir",
      "callToActionUrl": "#",
      "blockClass": [
        "gridHome",
        "gridHomeSoldes"
      ],
      "textAlignment": "left",
      "textMode": "rich-text"
    }
  },
  "toggle-layout#seoText": {
    "title": "SEO Text",
    "children": [
      "rich-text#seoHome"
    ]
  },
  "toggle-layout#spaceSoldes": {
    "title": "white space",
    "children": [
      "flex-layout.row#spaceSmall"
    ]
  },
  "timer-component#soldes": {
    "props": {
      "showDateFormat": false,
      "targetDate": "2022-11-25",
      "ore": "00",
      "minuti": "00",
      "secondi": "00",
      "Head": "flex-layout.row#soldes",
      // "Else": "flex-layout.row#ElseSoldes1",
      "blockClass": "2022"
    },
    "children": [
      "flex-layout.col#secondContainerSoldes"
