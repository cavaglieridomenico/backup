    },
    "children": ["product-highlight-text#promotionBadgeTextPDP"]
  },
  "vtex.product-highlights@2.x:product-highlights#extraBadgePromoPDP": {
    "title": "EXTRA BADGE PDP",
    "props": {
      "type": "collection",
      "blockClass": "promotionBadge",
      "filter": {
        "type": "show",
        "highlightNames": []
      }
    },
    "children": ["product-highlight-text#extraBadgeTextPDP"]
  },
  "product-highlight-text#promotionBadgeTextPDP": {
    "props": {
      "message": "{highlightName}",
      "blockClass": ["PromotionBadgeYellowPDP"]
    }
  },
  "product-highlight-text#extraBadgeTextPDP": {
    "props": {
      "message": "{highlightName}",
      "blockClass":  ["PromotionBadgeYellowPDP"
      // "extraBadge"
      ]
    }
  },
  "vtex.product-highlights@2.x:product-highlights#soldesBadge": {
    "props": {
      "type": "collection",
      "filter": {
        "type": "show",
        "highlightNames": ["SOLDES - pre-filtered PLP - 12/01-08/02"]
      },
      "blockClass": "promotionHighBlackFriday"
    },
    "children": ["product-highlight-text#soldesBadge"]
  },
  "product-highlight-text#soldesBadge": {
    "props": {
      "message": "Soldes"
    }
  },
  "vtex.product-highlights@2.x:product-highlights#Announcements": {
    "props": {
      "type": "collection",
      "filter": {
        "type": "show",
        "highlightNames": ["Collection Nouveautés"]
      },
      "blockClass": "promotionHigh"
    },
    "children": ["product-highlight-text#Announcements"]
  },
  "product-highlight-text#Announcements": {
    "props": {
      "message": "Nouveauté",
      "blockClass": ["greyText"]
    }
  },
  "flex-layout.row#stockFewPiecesLabelCard": {
    "children": ["condition-layout.product#stockFewPiecesLabel"],
    "props": {
      "blockClass": "availableBadgeCard"
    }
  }
}
