      "type": "collection",
      "blockClass": "promotionBadge",
      "filter": {
        "type": "show",
        "highlightNames": []
      }
    },
    "children": ["product-highlight-text#extraPromotionPLP"]
  },
  "vtex.product-highlights@2.x:product-highlights#badgePromoPDP": {
    "title": "CASHBACK/PROMO BADGE PDP",
    "props": {
      "type": "collection",
      "filter": {
        "type": "show",
