{
  "my-account-link.wishlist-link": {
    "props": {
      "label": "Ma liste de souhaits"
    }
  },
  "my-account-page.wishlist-page": {
    "children": [
      "link#backWishlist",
      "flex-layout.row#top",
      "list-context.wishlist"
    ]
  },
  "store.wishlist": {
    "blocks": [
      "link#backWishlist",
      "flex-layout.row#top",
      "list-context.wishlist",
      "flex-layout.row#newsletter-row"
    ]
  },
  "link#backWishlist": {
    "props": {
      "href": "https://frwhirlpool.myvtex.com/account",
      "label": "Retour",
      "blockClass": "backWishlist"
    }
  },
  "flex-layout.row#top": {
    "children": [
      "flex-layout.col#title"
    ]
  },
  "flex-layout.col#title": {
    "children": [
      "rich-text#titleWhish"
    ],
    "props": {
      "blockClass": "titleWishlist",
      "preventVerticalStretch": true
    }
  },
  "rich-text#titleWhish": {
    "props": {
      "text": "### Mes favoris"
    }
  },
  "list-context.wishlist": {
    "blocks": [
      "product-summary.shelf#caroselliLanding"
    ],
    "children": [
      "slider-layout-whl#carouselCollectionHome"
    ]
  },
  "slider-layout#wishlist": {
    "props": {
      "itemsPerPage": {
        "desktop": 3,
        "tablet": 1,
        "phone": 1
      },
      "showNavigationArrows": "desktopOnly",
      "showPaginationDots": "always",
      "infinite": false,
      "fullWidth": true,
      "blockClass": "shelf"
    }
  }
}