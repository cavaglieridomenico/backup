      "paddingTop": 7,
      "paddingBottom": 7,
      "blockClass": ["width1280Center", "mainRowPDP"]
    },
    "children": ["flex-layout.col#product-main-discontinued"]
  },
  "flex-layout.col#product-main-discontinued": {
    "props": {
      "colGap": 7,
      "rowGap": 7,
      "marginTop": 4,
      "marginBottom": 7,
      "paddingTop": 7,
      "paddingBottom": 7,
      "blockClass": ["width1280Center", "mainRowPDP"],
      "preserveLayoutOnMobile": true
    },
    "children": [
      "condition-layout.product#isAvailableStickyMobile-discontinued",
      "flex-layout.col#single-col-discontinued"
    ]
  },
  "condition-layout.product#isAvailableStickyMobile-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "DISPONIBILITÀ",
            "value": "Whirlpool Online Shop"
          }
        }
      ],
      "Then": "condition-layout.product#sticky-discontinuedMobile-discontinued"
    }
  },
  "condition-layout.product#sticky-discontinuedMobile-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#sticky-discontinued-with-priceMobile-discontinued"
    }
  },
  "condition-layout.product#sticky-discontinued-with-priceMobile-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#pdpSticky"
    }
  },
  "flex-layout.col#single-col-discontinued": {
    "props": {
      "blockClass": "product-desc-info",
      "preventVerticalStretch": true,
      "rowGap": 0
