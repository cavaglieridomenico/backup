    "children": [
      "flex-layout.row#product-main-mobile-discontinued",
      "toggle-layout#otherProductsCarousel"
    ]
  },
  "flex-layout.row#product-main-mobile-discontinued": {
    "props": {
      "colGap": 7,
      "rowGap": 7,
      "marginTop": 4,
