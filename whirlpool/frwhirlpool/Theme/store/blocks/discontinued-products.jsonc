    "title": "[ROW] - Other product carousel",
    "children": ["shelf.relatedProducts#otherProducts"],
    "props": {
      "blockClass": "pdpCarousels"
    }
  },
  "shelf.relatedProducts#otherProducts": {
    "blocks": ["product-summary.shelf"],
    "props": {
      "recommendation": "suggestions",
      "productList": {
        "maxItems": 10,
        "itemsPerPage": 5,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "showTitle": true,
        "titleText": "Inspirez-vous des autres modèles"
      },
      "blockClass": [
        "margin3125",
        "slideMargin",
        "slide20width",
        "mobileNomargin"
      ]
    }
  },
