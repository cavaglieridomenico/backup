/* Blocks can be defined both on the store/blocks.json file, and in any number of json
 * files inside the store/blocks directory, with any arbitrary directory structure.
 *
 * Additionally, all blocks files can support comments, such as this one, if given the
 * .jsonc file extension.
 *
 * The store/blocks.json file is intended for backwards compatibility, for miscellaneous
 * blocks, or if the app is simple enough that it doesn't warrant splitting the file.
 */
{
  "store.orderplaced": {
    "blocks": [
      "order-placed-custom",
      "flex-layout.row#newsletter-row"
    ]
  },
  "store.account": {
    "blocks": [
      "my-account" //"analytics"
    ],
    "parent": {
      "challenge": "challenge.profile"
    }
  },
  "store.not-found#product": {
    "children": [
      "rich-text#not-found-page",
      "rich-text#buttonNotFound",
      "image#404",
      "flex-layout.row#newsletter-row"
      //"analytics#error"
    ],
    "props": {
      "blockClass": [
        "flexDisplay",
        "flexColDir",
        "center"
      ]
    }
  },
  "store.not-found#search": {
    "children": [
      "rich-text#not-found-page-default",
      "rich-text#not-found-page-subtitle-default",
      "rich-text#not-found-page-retry",
      "flex-layout.row#searchbar404",
      "rich-text#not-found-page2",
      "rich-text#buttonNotFound",
      "rich-text#not-found-page-categories",
      "toggle-layout#mobileMenuCategory",
      "rich-text#not-found-page-contact",
      "slider-layout#linkCards-404",
      "flex-layout.row#newsletter-row"
      //"analytics#error"
    ],
    "props": {
      "blockClass": [
        "flexDisplay",
        "flexColDir",
        "center"
      ]
    }
  },
  "store.custom#contact": {
    "children": [
      "rich-text#not-found-page",
      "rich-text#buttonNotFound",
      "image#404",
      "flex-layout.row#newsletter-row"
      //"analytics"
    ],
    "props": {
      "blockClass": [
        "flexDisplay",
        "flexColDir",
        "center"
      ],
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "rich-text#not-found-page": {
    "props": {
      "text": "Désolé, aucun résultat",
      "textAlignment": "center",
      "blockClass": [
        "notFound",
        "center"
      ],
      "textPosition": "CENTER"
    }
  },
  "rich-text#buttonNotFound": {
    "props": {
      "text": "[ retour à la page d'accueil ](/)",
      "blockClass": [
        "buttonCard",
        "linkNotFound",
        "center"
      ],
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "rich-text#not-found-page-default": {
    "props": {
      "text": "Oups, cette page n'existe plus",
      "textAlignment": "CENTER",
      "blockClass": [
        "notFound"
      ],
      "textPosition": "CENTER"
    }
  },
  "rich-text#not-found-page-categories": {
    "props": {
      "text": "Catégories les plus recherchées",
      "textAlignment": "CENTER",
      "blockClass": [
        "notFoundTitle"
      ],
      "textPosition": "CENTER"
    }
  },
  "rich-text#not-found-page-contact": {
    "props": {
      "text": "Service consommateurs électroménagers",
      "textAlignment": "CENTER",
      "blockClass": [
        "notFoundTitle"
      ],
      "textPosition": "CENTER"
    }
  },
  "rich-text#not-found-page-retry": {
    "props": {
      "text": "Voulez-vous réessayer?",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["notFound3"]
    }
  },
  "rich-text#not-found-page-subtitle-default": {
    "props": {
      "text": "Le lien sembre cassé ou la page a été enlevée du site",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["notFound2"]
    }
  },
  "rich-text#not-found-page2": {
    "title": "[TEXT] - Result not found 2 ",
    "props": {
      "text": "Vous êtes à la recherche de plus de services? Découvrez notre [section service](/services/service-consommateurs), notre équipe sera ravie de vous aider",
      "textAlignment": "CENTER",
      "blockClass": ["notFound2"],
      "textPosition": "CENTER"
    }
  },
  "image#404": {
    "props": {
      "src": "assets/404.jpg",
      "blockClass": "center"
    }
  },
  "flex-layout.row#searchbar404": {
    "children": ["flex-layout.col#searchbar404"],
    "props": {
      "blockClass": "404searchBar"
    }
  },
  "flex-layout.col#searchbar404": {
    "children": ["search-bar#headerBlock"],
    "props": {
      "blockClass": "404searchBarCol"
    }
  },
  "slider-layout#linkCards-404": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 1
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": false
      },
      "blockClass": ["carousel"]
    },
    "children": [
      "flex-layout.row#linkCard1",
      "flex-layout.row#linkCard3",
      "flex-layout.row#linkCard5",
      "flex-layout.row#linkCard2"
    ]
  },
  "toggle-layout#mobileMenuCategory": {
    "props": {
      "visible": true
    },
    "children": [
      "responsive-layout.desktop#menuCategoryHome",
      "responsive-layout.tablet#menuCategoryHome",
      "responsive-layout.phone#menuCategoryHomeV2"
    ]
  },
  // "responsive-layout.desktop#menuCategoryHome": {
  //   "children": ["flex-layout.row#menuImagesHome"]
  // },
  // "responsive-layout.tablet#menuCategoryHome": {
  //   "children": ["flex-layout.row#menuImagesHome"]
  // },
  "responsive-layout.phone#menuCategoryHomeV2": {
    "children": [
      "slider-layout#homeCategoryProduct",
      "flex-layout.row#halfSpace"
    ]
  },
  /* ----- NEWSLETTER BLOCKS ----- */
  /* NEWSLETTER BUTTON */
  "flex-layout.row#newsletter-row": {
    "title": "[ROW BOTTOM] - Newsletter Row",
    "children": [
      "rich-text#newsletter-text",
      "newsletter-button"
    ],
    "props": {
      "blockClass": [
        "newsletterRow"
      ]
    }
  },
  "rich-text#newsletter-text": {
    "title": "Rich Text - Newsletter",
    "props": {
      "text": "#### Offres exclusives et remises",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": [
        "newsletterText"
      ]
    }
  },
  "newsletter-button": {
    "props": {
      "linkUrl": "/newsletter"
    }
  },
  /* NEWSLETTER AUTOMATIC POPUP */
  "flex-layout.row#newsletter-automatic-popup": {
    "title": "Row - Newsletter Automatic Popup",
    "children": [
      "newsletter-automatic-popup"
    ]
  },
  "newsletter-automatic-popup": {
    "props": {
      "time": 10000
    },
    "children": [
      "flex-layout.col#newsletter-form-automatic-popup"
    ]
  },
  /* NEWSLETTER FORM FOR THE AUTOMATIC POPUP */
  "flex-layout.col#newsletter-form-automatic-popup": {
    "title": "COL - Newsletter Form Automatic Popup",
    "children": [
      "experimental__visibility-layout#image-newsletter-form",
      "newsletter-form#automatic-popup"
    ],
    "props": {
      "blockClass": "newsletterFormCol"
    }
  },
  "newsletter-form#automatic-popup": {
    "props": {
      "title": "NEWSLETTER",
      "subtitles": [
        {
          "__editorItemTitle": "#### Vous désirez être avertis de nos bonnes affaires ? Alors n'hésitez pas à vous abonner à notre newsletter et soyez les premiers informés !"
        }
      ],
      "checkboxes": [
        {
          "checkboxLabel": "Je consens au traitement de mes données personnelles pour permettre à Whirlpool France S.A.S. de m'envoyer des bulletins d'information/communications marketing (sous forme électronique et non électronique, y compris par téléphone, courrier traditionnel, e-mail, SMS, MMS, notifications push sur des sites tiers, y compris sur les plateformes Facebook et Google) concernant les produits et services de Whirlpool France S.A.S. même achetés ou enregistrés par vous.",
          "checkboxRequired": true,
          "checkboxNewsletter": true,
          "checkboxError": "Cochez cette case pour continuer"
        }
      ],
      "privacyPolicyTextArray": [
        {
          "__editorItemTitle": "Je comprends le contenu de [Politique de protection des données personnelles](/pages/politique-de-protection-des-donnees-a-caractere-personnel) et:"
        }
      ],
      "errorRequiredField": "Complétez le champ",
      "errorInvalidMail": "Adresse email invalide",
      "successMessage": "Merci de vous être inscrit à notre newsletter !",
      "registeredErrorMessage": "Vous êtes déjà inscrit!",
      "pixelEventName": "newsletterAutomaticSubscription"
    }
  }
}
