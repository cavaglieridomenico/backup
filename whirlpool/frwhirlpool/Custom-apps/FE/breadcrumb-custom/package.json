{
  "name": "countdown",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,js,json}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,graphql,gql}": [
      "prettier --write"
    ]
  },
  "resolutions": {
    "graphql": "14.6.0",
    "expect": "28.1.3",
    "pretty-format": "28.1.3"
  } ,
  "devDependencies": {
    "@types/unorm": "^1.3.28",
    "@vtex/prettier-config": "0.1.4",
    "@vtex/tsconfig": "^0.4.4",
    "eslint": "7.12.1",
    "eslint-config-vtex": "12.3.2",
    "eslint-config-vtex-react": "^6.2.1",
    "husky": "4.2.3",
    "lint-staged": "10.1.1",
    "prettier": "2.0.2",
    "react-schemaorg": "^1.3.1",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "schema-dts": "^0.8.2",
    "unorm": "^1.6.0"
  }
}