{
  "store.custom#supportCC": {
    "blocks": [
      "flex-layout.row#breadcrumb-container",
      "flex-layout.row#supportCCrow"
    ]
  },
  "flex-layout.row#supportCCrow": {
    "children": [
      "flex-layout.col#supportCC"
    ],
    "props": {
      "blockClass": "supportCCrow"
    }
  },
  "flex-layout.col#supportCC": {
    "children": [
      "flex-layout.row#supportCCTitle",
      "flex-layout.row#supportCCTabLayout"
    ],
    "props": {
      "blockClass": "supportCCCol",
      "width": "100%"
    }
  },
  "flex-layout.row#supportCCTitle": {
    "children": [
      "rich-text#supportTitle"
    ],
    "props": {
      "blockClass": "supportCCTitle",
      "width": "100%"
    }
  },
  "flex-layout.row#supportCCTabLayout": {
    "children": [
      "responsive-layout.desktop#supportCCTabLayout",
      "responsive-layout.mobile#supportCCTabLayout"
    ],
    "props": {
      "blockClass": "supportCCTabLayout",
      "width": "100%"
    }
  },
  "responsive-layout.desktop#supportCCTabLayout": {
    "children": [
      "tab-layout#supportCC"
    ]
  },
  "rich-text#supportTitle": {
    "title": "Page Title",
    "props": {
      "text": "# Support",
      "blockClass": "supportPageTitle"
    }
  },
  "tab-layout#supportCC": {
    "children": [
      "tab-list#supportCC",
      "tab-content#supportCC"
    ],
    "props": {
      "blockClass": "supportTabCC",
      "defaultActiveTabId": "supportCC1"
    }
  },
  "tab-list#supportCC": {
    "props": {
      "blockClass": "supportCCListContainer"
    },
    "children": [
      "tab-list.item#supportCC1",
      "tab-list.item#supportCC2",
      "tab-list.item#supportCC3",
      "tab-list.item#supportCC4",
      "tab-list.item#supportCC5",
      "tab-list.item#supportCC6",
      "tab-list.item#supportCC7",
      "tab-list.item#supportCC8",
      "tab-list.item#supportCC9"
    ]
  },
  "tab-list.item#supportCC1": {
    "title": "List Item 1",
    "props": {
      "tabId": "supportCC1",
      "label": "Contactez-nous",
      "defaultActiveTab": true,
      "blockClass": [
        "listItemSupport",
        "listItem1"
      ]
    }
  },
  "tab-list.item#supportCC2": {
    "title": "List Item 2",
    "props": {
      "tabId": "supportCC2",
      "label": "FAQ",
      "blockClass": "listItemSupport"
    }
  },
  "tab-list.item#supportCC3": {
