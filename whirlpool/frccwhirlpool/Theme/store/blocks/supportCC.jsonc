  "tab-list.item#supportCC8": {
    "title": "List Item 8",
    "props": {
      "tabId": "supportCC8",
      "label": "Pour une panne",
      "blockClass": "listItemSupport"
    }
  },
  "tab-list.item#supportCC9": {
    "title": "List Item 9",
    "props": {
      "tabId": "supportCC9",
      "label": "Paiement",
      "blockClass": "listItemSupport"
    }
  },
  "tab-content#supportCC": {
    "props": {
      "blockClass": "supportCCContentContainer"
    },
    "children": [
      "tab-content.item#supportCC1",
      "tab-content.item#supportCC2",
      "tab-content.item#supportCC3",
