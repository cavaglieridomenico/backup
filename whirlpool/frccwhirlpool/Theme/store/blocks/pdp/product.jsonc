  },
  "flex-layout.row#suggestionsCarousel": {
    "children": [
      "shelf.relatedProducts#suggestions"
    ],
    "props": {
      "blockClass": "pdpCarousels"
    }
  },
  "flex-layout.row#showtogetherCarousel": {
    "children": [
      "shelf.relatedProducts#showtogether"
    ],
    "props": {
      "blockClass": "pdpCarousels"
    }
  },
  "condition-layout.product#suggestions": {
    "title": " [CONDITION] isDiscontinued ? false - relatedAccessories: suggestions",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#suggestionsCarousel"
    }
  },
  "condition-layout.product#showtogheter": {
    "title": " [CONDITION] isDiscontinued = false ? - relatedAccessories: showTogheter",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#showtogetherCarousel"
    }
  },
  "shelf.relatedProducts#showtogether": {
    "blocks": [
      "product-summary.shelf"
    ],
    "props": {
      "showPaginationDots": "never",
      "recommendation": "accessories",
      "productList": {
        "maxItems": 10,
        "itemsPerPage": 4,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "showTitle": true,
        "titleText": "Notre sélection pour compléter votre choix"
      },
      "blockClass": [
        "margin18",
        "slideMargin",
        "slide20width",
        "mobileNomargin"
      ]
    }
  },
  // #######------ finish relatedProducts ------#######
  // #######------ start promotionBadgePdp ------#######
  "flex-layout.row#product-highlights#promotion": {
