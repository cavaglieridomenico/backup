    // }
  },
  "responsive-layout.desktop#product-header": {
    "children": [
      "flex-layout.row#breadcrumb-container",
      "flex-layout.row#product-name-skuid",
      "flex-layout.row#addToWish-Desktop"
    ]
  },
  "flex-layout.row#product-name-skuid": {
    "children": [
      "flex-layout.col#title-col1",
      "flex-layout.col#title-col2",
      "flex-layout.col#title-col3"
    ],
    "props": {
      "blockClass": "stileskuid"
    }
  },
  "flex-layout.col#title-col1": {
    "children": [
      "vtex.store-components:product-name"
    ],
    "props": {
      "width": "50%"
    }
  },
  "flex-layout.col#title-col2": {
    "children": [
      // "flex-layout.row#stockAvailability"
      "flex-layout.row#lead-time"
    ],
    "props": {
      "width": "25%"
    }
  },
  "flex-layout.col#title-col3": {
    "children": [
      "product-identifier.product#productSkuId"
    ],
    "props": {
      "width": "25%"
    }
  },
  "flex-layout.row#stockAvailability": {
    "children": [
      "condition-layout.product#stockAvailability"
    ],
    "props": {
      "blockClass": "stockAvailability"
    }
  },
  "condition-layout.product#stockAvailability": {
    "title": " [CONDITION] isProductAvailable? inStock",
    "props": {
      "conditions": [
        {
          "subject": "isProductAvailable"
        }
      ],
      "Then": "flex-layout.row#enStockLabel",
      "Else": "flex-layout.row#notEnStockLabel"
    }
  },
  "flex-layout.row#enStockLabel": {
    "children": [
      "image#enStock",
      "rich-text#enStock"
    ],
    "props": {
      "blockClass": "stockAvailability"
    }
  },
  "image#enStock": {
    "props": {
      "src": "/arquivos/check-solid.svg",
      "width": "14px",
      "blockClass": "enStockImage"
    }
  },
  "rich-text#enStock": {
    "props": {
      "text": "En stock",
      "blockClass": "enStockLabel"
    }
  },
  "flex-layout.row#notEnStockLabel": {
    "children": [
      "image#notEnStock",
      "rich-text#notEnStock"
    ],
    "props": {
      "blockClass": "stockAvailability"
    }
  },
  "image#notEnStock": {
    "props": {
      "src": "/arquivos/times-solid.svg",
      "width": "14px",
      "blockClass": "enStockImage"
    }
  },
  "rich-text#notEnStock": {
    "props": {
      "text": "Not en stock",
      "blockClass": "enStockLabel"
    }
  },
  "flex-layout.row#addToWish-Desktop": {
    "children": [
      // "flex-layout.row#buttons-compareWishlist"
      "add-to-list-btn",
      "product-comparison-block.selector#pdp"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "addToWish-Desktop"
    }
  },
  "product-comparison-block.selector#pdp": {
    "props": {
      "blockClass": [
        "compareSearchPage",
        "compareSearchPdp"
      ]
    }
  },
  "responsive-layout.mobile#product-header": {
    "children": [
      "flex-layout.row#product-name-mobile",
      "flex-layout.row#stock-skuid-mobile",
      "flex-layout.row#productPreServizi-discontinued",
      "condition-layout.product#stickyAddToCart"
    ]
  },
  "flex-layout.row#product-name-mobile": {
    "props": {
      "blockClass": "productNameMobile"
    },
    "children": [
      "vtex.store-components:product-name"
    ]
  },
  "flex-layout.row#stock-skuid-mobile": {
    "children": [
      // "flex-layout.col#title-col6",
      // "flex-layout.col#title-col6",
      "flex-layout.col#title-col7"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "blockClass": "stileskuid"
    }
  },
  "flex-layout.col#title-col6": {
    "children": [
      // "flex-layout.row#stockAvailability"
      "flex-layout.row#lead-time"
    ],
    "props": {
      "width": "auto"
    }
  },
  "flex-layout.col#title-col7": {
    "children": [
      "product-identifier.product#productSkuId"
    ],
    "props": {
      "width": "auto"
    }
  },
  "product-identifier.product#productSkuId": {
    "props": {
      "idField": "skuReferenceId",
      "blockClass": "borderLeft",
      "product-identifier__label": "Test"
    }
  },
  // #######------ start xSell ------#######
  "condition-layout.product#hasAccessoryAssociationsNotStock": {
    "title": " [CONDITION] isProductAvailable? showAddToCartInPdp",
    "props": {
      "conditions": [
        {
          "subject": "isProductAvailable"
        }
      ],
      "Then": "condition-layout.product#hasAccessoryAssociations",
      "Else": "flex-layout.row#buy-button"
    }
  },
  "condition-layout.product#hasAccessoryAssociations": {
    "title": " [CONDITION] hasAccessoryAssociations = true ? - xSell Modal",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "hasAccessoryAssociations",
            // "value": true
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.binding#isEPP",
      // "Then": "rich-text#testAssociations"
      "Else": "flex-layout.row#buy-button"
    }
  },
  "condition-layout.binding#isEPP": {
    "title": " [CONDITION] isEpp = true ? hasAccessoriesInStockEpp",
    "props": {
      "conditions": [
        {
          "subject": "bindingId",
          "arguments": {
            "id": "1bbaf935-b5b4-48ae-80c0-346623d9c0c9"
          }
        }
      ],
      "Then": "condition-layout.product#hasAccessoriesEPP",
      "Else": "condition-layout.binding#isFF"
    }
  },
  "condition-layout.product#hasAccessoriesEPP": {
    "title": " [CONDITION] accessoriesHaveStockEPP = true ? xSell Modal",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "accessoriesHaveStockEPP",
            "value": "true"
          }
        }
      ],
      // "Then": "sticky-layout#addToCart-button"
      "Then": "modal-trigger#relatedAccessories",
      "Else": "condition-layout.product#hasAccessoriesFF"
    }
  },
  "condition-layout.binding#isFF": {
    "title": " [CONDITION] isFF ? true - hasAccessoriesInStockFF",
    "props": {
      "conditions": [
        {
          "subject": "bindingId",
          "arguments": {
            "id": "b9f7bf3a-c865-4169-8950-4fbb8b55ec09"
          }
        }
      ],
      "Then": "condition-layout.product#hasAccessoriesFF",
      "Else": "condition-layout.binding#isVIP"
    }
  },
  "condition-layout.product#hasAccessoriesFF": {
    "title": " [CONDITION] accessoriesHaveStockVIP = true ? xSell Modal",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "accessoriesHaveStockFF",
            "value": "true"
          }
        }
      ],
      "Then": "modal-trigger#relatedAccessories",
      "Else": "condition-layout.product#hasAccessoriesVIP"
    }
  },
  "condition-layout.binding#isVIP": {
    "title": " [CONDITION] isVip ? true - hasAccessoriesInStockVip",
    "props": {
      "conditions": [
        {
          "subject": "bindingId",
          "arguments": {
            "id": "17564333-171d-4290-998d-276aae4a3ee5"
          }
        }
      ],
      "Then": "condition-layout.product#hasAccessoriesVIP",
      "Else": "flex-layout.row#buy-button"
    }
  },
  "condition-layout.product#hasAccessoriesVIP": {
    "title": " [CONDITION] accessoriesHaveStockVIP = true ? xSell Modal",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "accessoriesHaveStockVIP",
            "value": "true"
          }
        }
      ],
      "Then": "modal-trigger#relatedAccessories",
      "Else": "flex-layout.row#buy-button"
    }
  },
  "rich-text#bindingFound": {
    "props": {
      "text": "binding found"
    }
  },
  // #######------ finish xSell ------#######
  // #######------ start relatedProducts ------#######
  "toggle-layout#similars": {
    "title": " [CONDITION] Toggle - Similar Items",
    "children": [
      "condition-layout.product#similars"
    ]
  },
  "toggle-layout#suggestions": {
    "title": " [CONDITION] Toggle - Suggestions",
