      "blockClass": "catalogCarouselImageContainer"
    }
  },
  "info-card#firstCarouselImage": {
    "props": {
      "id": "info-card-wcollection",
      "isFullModeStyle": true,
      "textPosition": "center",
      "textAlignment": "center",
      "imageUrl": "assets/bannerHp/image4.png",
      "mobileImageUrl": "assets/bannerHp/image4.png",
      "headline": "W-COLLECTION",
      "subhead": "Оцените возможности  «умного» приготовления пищи с технологией 6th SENSE**",
      "callToActionMode": "none",
      "blockClass": ["wcollectionCardhp", "catalogCarouselImage"]
    }
  },
  //CAROUSEL
  "flex-layout.col#firstCarousel": {
    "children": ["list-context.product-list#shelfBlockProductFirstCarousel"],
    "props": {
      "width": "75%",
      "blockClass": "carouselContainer"
    }
  },
  //TEXTS
  "flex-layout.row#firstCarouselTexts": {
    "children": [
      "flex-layout.col#firstCarouselTitle",
      "flex-layout.col#firstCarouselShowAll"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "preserveLayoutOnMobile": true,
      "blockClass": ["newProducts--row", "catalogCarouselTextsContainer"]
    }
  },
  "flex-layout.col#firstCarouselTitle": {
    "children": ["rich-text#firstCarouselTitle"],
    "props": {
      "blockClass": "newProducts-title"
    }
  },
  "flex-layout.col#firstCarouselShowAll": {
    "children": ["link#firstCarouselShowAll"],
    "props": {
      "blockClass": ["newProducts-seeAll-link", "catalog-seeAll-link"]
    }
  },
  "rich-text#firstCarouselTitle": {
    "props": {
      "text": "### Акции",
      "blockClass": "titleNew"
    }
  },
  "link#firstCarouselShowAll": {
    "props": {
      "label": "voir tout",
      "href": "/catalog/tovary-so-skidkoj",
      "blockClass": "linkSeeAll"
    }
  },
  //SLIDER
  "list-context.product-list#shelfBlockProductFirstCarousel": {
    "blocks": ["product-summary.shelf"],
    "children": ["slider-layout#catalogSlider"],
    "props": {
      "orderBy": "OrderByTopSaleDESC",
