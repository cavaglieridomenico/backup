  // DESKTOP buttons
  "flex-layout.col#pdp-desktop-buttons-qv": {
    "props": {
      "blockClass": "flexDesktopButtons"
    },
    "children": [
      "flex-layout.col#product-infos",
      "additional-services-pdp",
      "flex-layout.row#PriceAndBuyButtonQuickView"
    ]
  },
  "flex-layout.col#product-infos": {
    "children": ["flex-layout.row#productPreServizi-discontinued"],
    "props": {
      
    }
  },
  "flex-layout.row#PriceAndBuyButtonQuickView": {
    "props": {
      "blockClass": "PriceAndBuyButtonQuickView"
    },
    "children": [
      "flex-layout.col#quickviewPrice",
      "flex-layout.row#buy-button-quickview",
      "flex-layout.row#buy-button-go-to-checkout"
    ]
  },
  "condition-layout.product#subscription-online-qv": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "Subscription_field"
          }
        }
      ],
      "Then": "link.product#subscription-online-qv"
    }
  },
  "flex-layout.row#buy-button-quickview": {
    "props": {
      "marginTop": 4,
      "marginBottom": 7,
      "blockClass": ["AddToCArtPdp", "quickViewBtn"]
    },
    "children": ["add-to-cart-custom"]
  },
  "flex-layout.row#buy-button-go-to-checkout": {
    "props": {
      "marginTop": 4,
      "marginBottom": 7,
      "blockClass": ["AddToCArtPdp", "quickViewBtn", "goToCheckoutButton"]
    },
    "children": ["add-to-cart-custom#go-to-checkout"]
  },
  "add-to-cart-custom#go-to-checkout": {
    "props": {
      "isOneClickBuy": true,
      "text": "Acheter cet article",
      "blockClass": "goToCheckoutButton"
    }
  },
  "link.product#subscription-online-qv": {
    "props": {
      "displayMode": "button",
      "escapeLinkRegex": "[+*]",
      "buttonProps": {
        "variant": "secondary",
        "blockClass": "pdpsubonline"
      },
      "href": "https://application-v2-whirlpool.forward.lc/ru/new/{slug}",
      "label": "Acheter cet article",
      "blockClass": "pdpsubonline",
      "target": "_blank"
    }
  },
  "flex-layout.col#quickviewPrice": {
    "children": [
      "flex-layout.row#list-price-savings",
      "flex-layout.row#selling-price-quick"
    ],
    "props": {
      "blockClass": "quickviewPrice"
    }
  },
  "flex-layout.row#selling-price-quick": {
    "props": {
      "colGap": 2,
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": true,
      "marginBottom": 4
    },
    "children": ["product-selling-price#quick"]
  },
  "product-selling-price#quick": {
    "props": {
      "blockClass": "quick"
    }
  },
  "modal-actions#quickview": {
    "children": ["flex-layout.row#quickview-actions-1"]
  },
  "flex-layout.row#quickview-actions-1": {
    "children": ["flex-layout.col#quickview-add-to-card-button"]
  },
  "flex-layout.col#quickview-add-to-card-button": {
    "children": ["add-to-cart-custom"],
    "props": {
      "width": "grow"
    }
  },
  "product-specification-group#minimal": {
    "children": ["flex-layout.row#specifications"],
    "props": {
      "filter": {
        "specificationGroups": ["Endeca"],
        "type": "show"
      },
      "blockClass": "test-class-2"
    }
  },
  "flex-layout.row#specifications": {
    "children": ["product-specification#ps"],
    "props": {
      "marginTop": 3,
      "blockClass": "dimensionSpec"
    }
  },
  "product-specification#ps": {
    "children": ["flex-layout.col#specification-item"]
  },
  "flex-layout.col#specification-item": {
    "children": ["flex-layout.row#spec-item-quickview"]
  },
  "flex-layout.row#spec-item-quickview": {
    "props": {
      "blockClass": "descrDimensione"
    },
    "children": ["flex-layout.col#spec", "flex-layout.col#prod-spec-values"]
  },
  "flex-layout.col#spec": {
    "children": ["product-specification-text#specification"],
    "props": {
      "width": "60%"
    }
  },
  "flex-layout.col#prod-spec-values": {
    "children": ["product-specification-values"],
    "props": {
      "width": "40%"
    }
  }
