{
  "modal-trigger#quickview": {
    "children": ["image#iconQuickView", "modal-layout#quickview"],
    "props": {
      "blockClass": "quickview"
    }
  },
  "image#iconQuickView": {
    "props": {
      "src": "assets/icons/QuickviewIcon.svg",
      "blockClass": "quickViewIcon"
    }
  },
  "modal-layout#quickview": {
    "children": ["modal-header#quickview", "modal-content#quickview-main"],
    "props": {
      "blockClass": "quickview"
    }
  },
  "rich-text#modal-content": {
    "props": {
      "text": "Open modal"
    }
  },
  "modal-header#quickview": {
    "props": {
      "blockClass": "quickview"
    }
  },
  "modal-content#quickview-main": {
    "children": [
      "flex-layout.row#quickview-productName-availability",
      "flex-layout.row#quickview-main-row"
    ],
    "props": {
      "blockClass": "quickview-main"
    }
  },
  //first row
  "flex-layout.row#quickview-productName-availability": {
    "props": {
      "blockClass": "quickview-firstRow",
      "preventHorizontalStretch": true
    },
    "children": [
      "flex-layout.col#quickview-product-name",
      "flex-layout.col#quickview-availability"
    ]
  },
  "flex-layout.col#quickview-product-name": {
    "props": {
      "blockClass": "quickview-product-name"
    },
    "children": ["vtex.store-components:product-name"]
  },
  "flex-layout.col#quickview-availability": {
    "props": {
      "blockClass": "imageProductAvailability" //"quickview-availability"
    },
    "children": [
      "product-availability#quickView",
      "flex-layout.row#quickview-subtitle"
    ]
  },
  "product-availability#quickView": {
    "props": {
      "threshold": "1",
      "lowStockMessage": "{quantity} pieces", // Only {quantity} left!
      "highStockMessage": "En stock" // in stock
    }
  },
  //second row
  "flex-layout.row#quickview-subtitle": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "quiclViewSubtitle"
    },
    "children": [
      "flex-layout.col#quickview-subtitle1",
      "flex-layout.col#quickview-subtitle2"
    ]
  },
  "flex-layout.col#quickview-subtitle1": {
    "props": {
      "verticalAlign": "middle" 
    },
    "children": [
      // "add-to-list-btn"
    ]
  },
  "flex-layout.col#quickview-subtitle2": {
    "props": {
      "verticalAlign": "middle"
    },
    "children": ["product-identifier.product#productSkuId-quickview"]
  },
  "product-identifier.product#productSkuId-quickview": {
    "props": {
      "idField": "skuReferenceId"
    }
  },
  //main row
  "flex-layout.row#quickview-main-row": {
    "children": [
      "flex-layout.col#quickview-images",
      "flex-layout.col#quickview-product-details"
    ],
    "props": {
      "blockClass": "quickviewMainRow",
      "preventHorizontalStretch": false,
      "preventVerticalStretch": true
    }
  },
  "flex-layout.col#quickview-images": {
    "children": [
      "video-player-thron-pdp#quickview",
      "flex-layout.row#product-specification",
      "flex-layout.row#minimal",
      "flex-layout.row#product-details"
    ]
  },
  "video-player-thron-pdp#quickview": {
    "props": {
      "blockClass": "quickview-images",
      "displayMode": "carousel",
      "showNavigationArrows": true,
      "showPaginationDots": false
    }
  },
  "flex-layout.row#product-specification": {
    "props": {
      "blockClass": "quickViewSpecTitle"
    },
    "children": ["rich-text#quickview-product-specification"]
  },
  "flex-layout.row#minimal": {
    "children": ["product-specification-group#minimal"]
  },
  "rich-text#quickview-product-specification": {
    "props": {
      "blockClass": "quickview-product-specification-text",
      "text": "Caractéristiques"
    }
  },
  "flex-layout.row#product-details": {
    "children": ["link.product#seeDetail"]
  },
  "link.product#seeDetail": {
    "props": {
      "href": "/{slug}/p",
      "escapeLinkRegex": "[+*]",
      "label": "Voir toutes les caractéristiques",
      "blockClass": "seeDetails"
    }
  },
  "flex-layout.col#quickview-product-details": {
    "children": [
      "flex-layout.col#quickview-product-details-content",
      "flex-layout.col#pdp-desktop-buttons-qv"
      //"flex-layout.col#quickview-add-to-card-button"
      //"modal-actions#quickview"
    ],
    "props": {
      "preventVerticalStretch": true,
      "blockClass": ["rightBlock", "quickViewRightBlock"]
    }
  },
  "flex-layout.col#quickview-product-details-content": {
    "children": ["flex-layout.row#included-in-price-title"],
    "props": {
      "preventVerticalStretch": true
    }
  },
  "flex-layout.row#included-in-price-title": {
    "children": [
      // "free-additional-services#quickview"
    ]
  },
  "rich-text#included-in-price": {
    "props": {
      "text": "Included in price"
    }
  },
