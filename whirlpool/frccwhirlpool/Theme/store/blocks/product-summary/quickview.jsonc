  // quick condition layout subscription
  "condition-layout.product#quickCondSubTitle": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "MonthlyRate"
          }
        }
      ],
      "Then": "flex-layout.row#quickSubscription_price_per_mounth"
    }
  },
  "flex-layout.row#quickSubscription_price_per_mounth": {
    "children": [
      "print-specification#month-price",
      "flex-layout.row#quickTooltipsubscription"
    ],
    "props": {
      "colSizing": "auto",
      "preserveLayoutOnMobile": true,
      "blockClass": ["subscriptionRow", "subscriptionRowMonth"]
    }
  },
  "flex-layout.row#quickTooltipsubscription": {
    "children": ["tooltip#subscription"],
    "props": {
      "blockClass": "subscriptionTooltip"
    }
  },
  "product-specification-group#quickSubscriptionValue": {
    "children": ["product-specification#quickSubscriptionValueCode"],
    "props": {
      "filter": {
        "specificationGroups": ["Leasing"],
        "type": "show"
      }
    }
  },
  "product-specification#quickSubscriptionValueCode": {
    "children": ["product-specification-values#quickSubscriptionValueField"]
  },
  "product-specification-values#quickSubscriptionValueField": {
    "children": ["product-specification-text#quickSubscriptionFieldValue"]
  },
  "product-specification-text#quickSubscriptionFieldValue": {
    "props": {
      "message": "{specificationValue}",
      "blockClass": "subscriptionText"
    }
  },
