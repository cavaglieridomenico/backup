{
  "header": {
    "title": ["HEADER"],
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  "header.full": {
    "blocks": ["header-layout.desktop", "header-layout.mobile"]
  },
  /*------------ DESKTOP ------------*/
  "header-layout.desktop": {
    "children": [
      "experimental__visibility-layout#stripe-custom",
      "sticky-layout#logoSearchSupport",
      "custom-catalog-menu-desktop"
      //"experimental__visibility-layout#propbanner"
    ]
  },
  "custom-catalog-menu-desktop": {
    "children": ["print-vip-logos"]
  },
  //First Row
  "sticky-layout#logoSearchSupport": {
    "title": "[STICKY] Main Header",
    "props": {
      "blockClass": "logoSearchSupport"
    },
    "children": ["flex-layout.row#logoSearchSupport"]
  },
  "flex-layout.row#logoSearchSupport": {
    "title": "[ROW/DESKTOP] Main Header Row",
    "props": {
      "blockClass": "logoSearchSupport"
      // "preventHorizontalStretch": true,
      // "preventVerticalStretch": true,
      // "fullWidth": true
    },
    "children": [
      "flex-layout.col#logoBrandDesktop",
      "flex-layout.col#searchBarDesktop",
      //"flex-layout.col#newsletterDesktop",
      "flex-layout.col#headerLoginSupportDesktop",
      "flex-layout.col#headerWishListCartDesktop"
    ]
  },
  //Logo
  "flex-layout.col#logoBrandDesktop": {
    "title": "[COL/DESKTOP] Brand Logo Col",
    "props": {
      "blockClass": "logoBrandDesktop",
      "width": "15%"
    },
    "children": ["logo#header"]
  },
  "logo#header": {
    "title": "[LOGO] Brand Logo",
    "props": {
      "title": "Logo",
      "href": "/",
      "url": "assets/icons/logo-header.svg",
      "blockClass": "logoHeaderDesktop"
    }
  },
  //SearchBar
  "flex-layout.col#searchBarDesktop": {
    "title": "[COL/DESKTOP] Search bar Col",
    "props": {
      "blockClass": "searchBarDesktop",
      "width": "50%"
    },
    "children": ["search-bar"]
  },
  //Newsletter
  "flex-layout.col#newsletterDesktop": {
    "title": "[COL/DESKTOP] Newsletter Col",
    "props": {
      "blockClass": "newsletterDesktop",
      "width": "20%"
    },
    "children": ["newsletter-popup#headerMobile"]
  },
  //Support
  "flex-layout.col#headerLoginSupportDesktop": {
    "title": "[COL/DESKTOP] Icons row",
    "props": {
      "blockClass": "headerSupportDesktopCol",
      "width": "35%"
    },
    "children": ["flex-layout.row#headerLoginSupportDesktop"]
  },
  "flex-layout.row#headerLoginSupportDesktop": {
    "title": "[ROW/DESKTOP] Icons row",
    "props": {
      "blockClass": "headerSupportDesktopRowLoginSupport",
      "colSizing": "auto"
    },
    "children": ["conditional-login", "link#supportDesktop"]
  },
  "conditional-login": {
    "props": {
      "loginPageUrl": "/authentication"
    }
  },
  //Whishlist - Cart
  "flex-layout.col#headerWishListCartDesktop": {
    "title": "[COL/DESKTOP] Icons row",
    "props": {
      "blockClass": "headerSupportDesktopCol",
      "width": "35%"
    },
    "children": ["flex-layout.row#headerSupportDesktop"]
  },
  "flex-layout.row#headerSupportDesktop": {
    "title": "[ROW/DESKTOP] Icons row",
    "props": {
      "blockClass": "headerSupportDesktopRowWishlistCart",
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#headerWishListLabel",
      "flex-layout.col#headerCartLabel"
    ]
  },
  "flex-layout.col#headerWishListLabel": {
    "title": "[COL/DESKTOP] Icons row",
    // "props": {
    //   "blockClass": "headerSupportDesktopCol",
    // },
    "children": [
      "condition-layout.binding#invitationIcon",
      "rich-text#wishlist"
    ]
  },
  "flex-layout.col#headerCartLabel": {
    "title": "[COL/DESKTOP] Icons row",
    // "props": {
    //   "blockClass": "headerSupportDesktopCol",
    // },
    "children": ["minicart.v2#headerMobile", "rich-text#minicart"]
  },
  "condition-layout.binding#invitationIcon": {
    "props": {
      "conditions": [
        {
          "subject": "bindingId",
          "arguments": {
            "id": "f024e427-bac9-4462-8a77-db3bcaa99be6"
          }
        }
      ],
      "Then": "link#wishlistDesktop",
      "Else": "invitation-condition-layout#invitationIcon"
    }
  },
  "invitation-condition-layout#invitationIcon": {
    "title": "[CUSTOM] Invitation condition layout",
    "props": {
      "propertyToCheck": "invitations",
      "Then": "link#F&FDesktop",
      "Else": "link#wishlistDesktop"
    }
  },
  "link#phoneDesktop": {
    "title": "[LINK] Phone link",
    "props": {
      "href": "tel:03448224224",
      "isAnalyticsEvent": true,
      "analyticsEventName": "contacts_click",
      "analyticsType": "telephone",
      "isPrevent": false,
      "clickArea": "footer",
      "blockClass": ["phoneDesktop", "phoneMobileLabel", "phoneMobileLabelTop"]
    },
    "children": ["logo#phoneMobile"]
  },
  "link#supportDesktop": {
    "title": "[LINK] Support link",
    "props": {
      "href": "/support",
      "label": "Support",
      "blockClass": [
        "helpDesktop",
        "phoneMobileLabel",
        "phoneMobileLabelTop",
        "SupportCTA"
      ]
    }
  },
  "link#F&FDesktop": {
    "title": "[LINK] F&F link",
    "props": {
      "href": "/account#/friends",
      "blockClass": ["FFDesktop", "phoneMobileLabel", "phoneMobileLabelTop"]
    },
    "children": ["logo#phoneMobile"]
  },
  "link#wishlistDesktop": {
    "title": "[LINK] wishlist link",
    "props": {
      "href": "/account#/wishlist",
      "blockClass": [
        "wishlistDesktop",
        "phoneMobileLabel",
        "phoneMobileLabelTop"
      ]
    },
    "children": ["logo#phoneMobile"]
  },
  "rich-text#wishlist": {
    "title": "[TEXT] Wishlist text",
    "props": {
      "blockClass": "wishlistLabel",
      "text": "Wishlist"
    }
  },
  "rich-text#minicart": {
    "title": "[TEXT] Cart text",
    "props": {
      "blockClass": "cartLabel",
      "text": "Cart"
    }
  },
  "menu-item#help": {
    "props": {
      "id": "menu-item-help",
      "type": "custom",
      "highlight": false,
      "blockClass": "top-menu",
      "itemProps": {
        "type": "internal",
        "href": "/support",
        "noFollow": false,
        "tagTitle": "Support",
        "text": "Support"
      },
      "iconProps": {
        "id": "inf-info",
        "size": 16,
        "viewBox": "0 0 16 16"
      },
      "iconToTheRight": true
    }
  },
  "menu-item#wishlist": {
    "props": {
      "id": "menu-item-wishlist",
      "type": "custom",
      "iconId": null,
      "highlight": false,
      "blockClass": "top-menu",
      "itemProps": {
        "type": "internal",
        "href": "/account#/wishlist",
        "noFollow": false,
        "tagTitle": "Wishlist",
        "text": "Wishlist"
      },
      "iconProps": {
        "id": "inf-heart",
        "size": 16,
        "viewBox": "-1.5 -1 22 20"
      },
      "iconToTheRight": true
    }
  },
  /* ------------ STRIPE CUSTOM--------------- */
  "experimental__visibility-layout#stripe-custom": {
    "title": "[TOGGLE] - [TOP] Toggle Stripe POPUP",
    "props": {
      "visible": false
