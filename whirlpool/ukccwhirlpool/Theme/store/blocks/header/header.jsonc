  "image#stripeImage": {
    "title": "[IMAGE] - [TOP] Stripe Image",
    "props": {
      //"blockClass": "popupHome",
      "alt": "icon of stripe",
      "src": "/arquivos/ph-logo.png",
      "maxWidth": 24,
      "maxHeight": 24
    }
  },
  "rich-text#stripeText": {
    "title": "[TEXT] - [TOP] Stripe Text",
    "props": {
      "blockClass": "stripeText",
      "text": "TESTO DI PROVA DELLO STRIPE - POP UP CUSTOM IN HOMEPAGE",
      "textAlignment": "LEFT",
      "textPosition": "CENTER"
    }
  },
  "experimental__visibility-layout#stripeLink": {
    "title": "[TOGGLE] - [TOP] Stripe Link",
    "props": {
      "visible": true
    },
    "children": ["link#stripeLink"]
  },
  "link#stripeLink": {
    "title": "[LINK] - [TOP] Stripe Link",
    "props": {
      "blockClass": "stripeLink",
      "label": "ACQUISTA ORA",
      "href": "#",
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "small"
      },
      "isPrevent": false
    }
  },
