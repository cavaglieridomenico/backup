    "props": {
      "blockClass": "deliverySimulatorCol"
    }
  },
  "condition-layout.product#isDiscontinuedShippingSimulatorQuestion": {
    "title": "[CONDITION] isDiscontinued = true ? - shippingSim",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "true"
          }
        }
      ],
      "Then": null,
      "Else": "flex-layout.row#shippingSimulatorQuestion"
    }
  },
  "flex-layout.row#shippingSimulatorQuestion": {
    "children": [
      "rich-text#shippingSimulatorQuestion"
    ],
    "props": {
      "blockClass": "shippingSimulatorQuestion"
    }
  },
  "rich-text#shippingSimulatorQuestion": {
    "props": {
      "text": "Check if your delivery zip code is covered for installation:",
      "blockClass": "shippingSimulatorQuestion"
    }
  },
  "condition-layout.product#isDiscontinuedShippingCustom": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "true"
          }
        }
      ],
      "Then": null,
      "Else": "shipping-custom-pdp"
    }
