    ]
  },
  "condition-layout.product#wpa-features": {
    "title": " [CONDITION] productType = 'professional' - wpaFeatures",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "productType",
            "value": "professional"
          }
        }
      ],
      "Then": "product-specification-group#features-wpa",
      "Else": "product-specification-group#features-standard"
    }
  },
  "product-specification-group#features-wpa": {
    "children": [
      "flex-layout.col#technical-features"
    ],
    "props": {
      "filter": {
        "specificationGroups": [
          "Special Features"
        ],
        "type": "show"
      }
    }
  },
  "flex-layout.col#technical-features": {
    "children": [
      "product-specifications-with-images"
    ],
    "props": {
      "blockClass": "productSpecificationFeatures"
    }
  },
  "product-specification-group#features-standard": {
    "children": [
      "flex-layout.col#technical-features"
    ],
    "props": {
      "filter": {
        "specificationGroups": [
          "RatingGroupAttrLogo"
        ],
        "type": "show"
      }
    }
  },
  //Features Accordion
  "disclosure-layout#features": {
    "title": "[DISCLOSURE] - Faetures layout",
    "props": {
      "blockClass": "pdpAccordion"
    },
    "children": [
      "disclosure-trigger#features",
      "disclosure-content#features"
    ]
