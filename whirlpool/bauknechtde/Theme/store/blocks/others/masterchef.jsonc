{
  "store.custom#masterchef": {
    "children": [
      "flex-layout.row#bread-theme",
      "experimental__visibility-layout#heroBannerMasterchef",

      //"analytics",
      "__fold__.mobile",
      // "flex-layout.row#space",
      "experimental__visibility-layout#halfBannerMasterchef",
      "flex-layout.row#space",
      "flex-layout.row#countdownStripe",
      "experimental__visibility-layout#imageSlide",
      "experimental__visibility-layout#imageSlideReverse",
      "experimental__visibility-layout#imageSlideReverse2",
      "experimental__visibility-layout#imageSlide1",
      "experimental__visibility-layout#title3",
      "list-context.product-list#carouselMasterchef",
      "flex-layout.row#greyBar"
    ]
  },
  "experimental__visibility-layout#heroBannerMasterchef": {
    "props": {
      "visible": true
    },
    "children": [
      "info-card#heroBannerMasterchef",
      "flex-layout.row#heroBannerMasterchef"
    ]
  },
  "experimental__visibility-layout#halfBannerMasterchef": {
    "props": {
      "visible": true
    },
    "children": [
      "info-card#halfBannerMasterchef",
      "flex-layout.row#halfBannerMasterchef"
    ]
  },
  "flex-layout.row#heroBannerMasterchef": {
    "props": {
      "blockClass": "heroBannerMasterchef"
    },
    "children": [
      "image#logoHotpoint",
      "rich-text#logoHotpoint",
      "rich-text#titleHotpoint",
      "image#logoMasterchef"
    ]
  },
  "flex-layout.row#halfBannerMasterchef": {
    "props": {
      "blockClass": ["halfBannerMasterchef", "halfBannerMasterchef"]
    },
    "children": [
      "rich-text#halfBannerTitleHotpoint",
      "rich-text#halfbannerSubTitle"
    ]
  },
  "image#logoHotpoint": {
    "props": {
      "src": "assetslogos/hotpoint-logo.svg",
      "width": "6.25rem",
      "blockClass": "logoHotpointMasterchef"
    }
  },
  "image#logoMasterchef": {
    "props": {
      "src": "assetslogos/hotpoint-logo.svg",
      "width": "6.25rem",
      "blockClass": "logoMasterchef"
    }
  },
  "rich-text#logoHotpoint": {
    "props": {
      "text": "# Gli elettrodomestici scelti da",
      "blockClass": "titleMasterchef"
    }
  },
  "rich-text#halfbannerSubTitle": {
    "props": {
      "text": "# Prestazioni elevate per risultati professionali",
      "blockClass": [
        "titleMasterchef",
        "halfBannerMasterchef",
        "halfBannerMasterchefSubTitle"
      ]
    }
  },
  "rich-text#titleHotpoint": {
    "props": {
      "text": "# Masterchef Italia",
      "blockClass": ["titleMasterchef", "titleMasterchefTitle"]
    }
  },
  "rich-text#halfBannerTitleHotpoint": {
    "props": {
      "text": "# Elettrodomestici Hotpoint",
      "blockClass": [
        "titleMasterchef",
        "titleMasterchefTitle",
        "halfBannerMasterchef"
      ]
    }
  },
  "info-card#heroBannerMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/ph-square.jpg",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["heroBannerChef", "heroBannerSearchChef"],
      "textAlignment": "left"
    }
  },
  "info-card#halfBannerMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/ph-square.jpg",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["halfBannerChef", "halfBannerSearchChef"],
      "textAlignment": "left"
    }
  },
  "flex-layout.row#countdownStripe": {
    "props": {
      "fullWidth": true,
      "blockClass": ["countdownStripe"]
    },
    "children": ["rich-text#countdownStripe", "rich-text#countdownStripeDown"]
  },
  "flex-layout.row#greyBar": {
    "props": {
      "fullWidth": true,
      "blockClass": ["greyBar"]
    },
    "children": ["rich-text#greyBar"]
  },
  "rich-text#countdownStripe": {
    "props": {
      "text": "# I protagonisti del programma",
      "blockClass": ["countdownStripeTitle"]
    }
  },
  "rich-text#greyBar": {
    "props": {
      "text": "# *© 2020 Shine TV Limited. MasterChef and the MasterChef logo are registered trademarks of Shine TV Limited and its affiliates. Licensed by Endemol Shine IP B.V. All Rights Reserved.",
      "blockClass": ["greyBarTitle"]
    }
  },
  "rich-text#countdownStripeDown": {
    "props": {
      "blockClass": ["countdownStripeDown"]
    }
  },
  "experimental__visibility-layout#imageSlide": {
    "props": {
      "visible": true
    },
    "children": ["info-card#imageSlideMasterchef"]
  },
  "experimental__visibility-layout#imageSlide1": {
    "props": {
      "visible": true
    },
    "children": ["info-card#imageSlideMasterchef1"]
  },
  "experimental__visibility-layout#imageSlideReverse": {
    "props": {
      "visible": true
    },
    "children": ["info-card#imageSlideReverseMasterchef"]
  },
  "experimental__visibility-layout#imageSlideReverse2": {
    "props": {
      "visible": true
    },
    "children": ["info-card#imageSlideReverseMasterchef2"]
  },
  "info-card#imageSlideMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "imageUrl": "assets/ph-square.jpg",
      "headline": "Forno Active Steam 75 <br><br> Massima versatilità di cottura",
      "subhead": "I forni Hotpoint permetteranno agli aspiranti chef di cuocere i propri piatti con 3 diversi livelli di vapore (75%, 50%, 25%) oppure potranno combinare vapore e cottura tradizionale, presentando ai giudici piatti gustosi ma anche salutari.",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["imageSlideChef"]
    }
  },
  "info-card#imageSlideMasterchef1": {
    "props": {
      "id": "hero-banner-main-search",
      "imageUrl": "assets/ph-square.jpg",
      "headline": "Forno Active Steam 75 <br><br> Massima versatilità di cottura",
      "subhead": "I forni Hotpoint permetteranno agli aspiranti chef di cuocere i propri piatti con 3 diversi livelli di vapore (75%, 50%, 25%) oppure potranno combinare vapore e cottura tradizionale, presentando ai giudici piatti gustosi ma anche salutari.",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["imageSlideChef"]
    }
  },
  "info-card#imageSlideReverseMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "imageUrl": "assets/ph-square.jpg",
      "headline": "Forno Active Steam 75 <br><br> Massima versatilità di cottura",
      "subhead": "I forni Hotpoint permetteranno agli aspiranti chef di cuocere i propri piatti con 3 diversi livelli di vapore (75%, 50%, 25%) oppure potranno combinare vapore e cottura tradizionale, presentando ai giudici piatti gustosi ma anche salutari.",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["imageSlideChef", "imageSlideChefReverse"]
    }
  },
  "info-card#imageSlideReverseMasterchef2": {
    "props": {
      "id": "hero-banner-main-search",
      "imageUrl": "https://hotpointit.vtexassets.com/assets/vtex.file-manager-graphql/images/8b08b14e-dc56-4ebf-bbe5-9110d21a16a4___70ba7e5f16ea303bb9d93b07ccba1873.jpg",
      "headline": "PIANO INDUZIONE DA 60 CM <br> <br> COTTURA INTUITIVA",
      "subhead": "Durante le sfide il tempo scorre veloce! La funzione Active Cook dei piani cottura ad induzione Hotpoint permetterà ai nostri cuochi amatoriali di raggiungere sempre i risultati desiderati nel modo più intuitivo e veloce...aggiudicandosi così la vittoria.",
      "callToActionMode": "CallToActionEnum",
      "blockClass": ["imageSlideChef", "imageSlideChefReverse"]
    }
  },
  "list-context.product-list#carouselMasterchef": {
    "blocks": ["product-summary.shelf#caroselliLanding"],
    "children": ["slider-layout#masterchef"],
    "props": {
      "collection": "151"
      //   "productList": {
      //     "maxItems": 10,
      //     "itemsPerPage": 5,
      //     "minItemsPerPage": 1,
      //     "scroll": "BY_PAGE",
      //     "arrows": true,
      //     "titleText": "Non sai da dove iniziare? Scopri i prodotti Whirlpool più venduti"
      // }
    }
  },
  "slider-layout#masterchef": {
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 3,
        "phone": 1
      },
      "showNavigationArrows": "desktopOnly",
      "infinite": true,
      "fullWidth": false,
      "blockClass": ["shelf", "marginBottom2rem"]
    }
  }
}
