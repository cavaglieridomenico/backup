  /*-------------------- SHOW PRICE --------------------*/
  "condition-layout.product#showPriceUnavailableProdPLPBK": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#price-summary-spare"
    }
  },
  "product-selling-price#summaryBK": {
    "props": {
      "alwaysShow": true,
      "blockClass": ["summary", "summaryPL"],
      "markers": ["Tax"],
      "message": "{sellingPriceValue} Inkl. MwSt."
    }
  },
  /*-------------------- SAVINGS PRICE --------------------*/
  "condition-layout.product#showSavingsPriceUnavailableProdPLPBK": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "true"
          }
        }
      ],
      "Then": "flex-layout.row#savingPricesPLPBK"
    }
  },
  "flex-layout.row#savingPricesPLPBK": {
    "props": {
      "blockClass": "savingPricesPLPBK",
      "colSizing": "auto"
    },
    "children": [
      "flex-layout.col#listPricePLPBK",
      "flex-layout.col#savingPLPBK"
    ]
  },
  "flex-layout.col#listPricePLPBK": {
    "props": {
      "blockClass": "listPricePLPBKBox"
    },
    "children": ["product-list-price#listPricePLPBK"]
  },
  "flex-layout.col#savingPLPBK": {
    "props": {
      "blockClass": "savingPLPBKBox"
    },
    "children": [
      "product-price-savings#savingPLPBK",
      "product-spot-price-savings#percentagePLPBK"
    ]
  },
  "product-spot-price-savings#percentagePLPBK": {
    "props": {
      "alwaysShow": true,
      "blockClass": "percentagePLPBK",
      "message": "-{spotPriceSavingsPercentage}"
    }
  },
  "product-price-savings#savingPLPBK": {
    "props": {
      "markers": ["highlight"],
      "blockClass": "savingPLPBK",
      "message": "Rabatt: {savingsValue}"
    }
  },
  "product-list-price#listPricePLPBK": {
    "props": {
      "alwaysShow": true,
      "blockClass": "listPricePLPBK"
    }
  }
}
