{
  "slider-layout#banner-carousel": {
    "title": "[LAYOUT] - [TOP] - Slider Below Header",
    "props": {
      "itemsPerPage": {
        "desktop": 4,
        "tablet": 4,
        "phone": 1
      },
      "infinite": false,
      "autoplay": {
        "timeout": 3000
      },
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "bannerHeaderContainer"
    },
    "children": [
      "flex-layout.row#prima",
      "flex-layout.row#seconda",
      "flex-layout.row#terza",
      "flex-layout.row#quarta"
    ]
  },

  "flex-layout.row#prima": {
    "title": "[ROW] - [TOP] - Element 1 Container Below Header",
    "props": {
      "blockClass": ["bannerheader", "BorderCenterElements"],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["image#logoBannerHeader1", "rich-text#BannerHeaderText1"]
  },
  "image#logoBannerHeader1": {
    "title": "[IMAGE] - [TOP] - Element 1 Image Below Header",
    "props": {
      "src": "assets/icons/consegna.svg",
      "width": "24px",
      "blockClass": "sliderBanner"
    }
  },
  "rich-text#BannerHeaderText1": {
