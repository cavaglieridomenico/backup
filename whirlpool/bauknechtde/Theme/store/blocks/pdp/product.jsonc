  "responsive-layout.desktop#suggestedCarousel": {
    "title": "[Responsive] - [Desktop] - Suggested Carousel",
    "children": ["condition-layout.product#notSpareOrAccessory"]
  },
  "condition-layout.product#notSpareOrAccessory": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isSparePart",
            "value": "true"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        }
      ],
      "matchType": "any",
      "Else": "condition-layout.product#isSellableForCarousel"
    }
  },
  "responsive-layout.desktop#suggestionsDesktop": {
    "children": ["condition-layout.product#suggestionsDesktop"]
  },
  "responsive-layout.mobile#suggestionsMobile": {
    "children": ["condition-layout.product#suggestions"]
  },
  "condition-layout.product#isSellableForCarousel": {
    "title": "[Condition] - Is sellable? To show suggested carousel",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#isOutOfStockForCarousel",
      "Else": "condition-layout.product#isDiscontinuedForCarousel"
    }
  },
  "condition-layout.product#isOutOfStockForCarousel": {
    "title": "[Condition] - Is out of stock? For suggested carousel",
    "props": {
      "conditions": [
        {
          "subject": "isProductAvailable"
        }
      ],
      "Else": "flex-layout.row#suggestedCarouselOutOfStock"
    }
  },
  "condition-layout.product#isDiscontinuedForCarousel": {
    "title": "[Condition] - Is discontinued? For suggested carousel",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#suggestedCarouselUnsellable"
    }
  },
  "flex-layout.row#suggestedCarouselOutOfStock": {
    "title": "[Row] - [MID] - Suggested Carousel oos",
    "props": {
      "blockClass": "suggestedCarousel"
    },
    "children": ["flex-layout.col#suggestedCarouselOutOfStock"]
  },
  "flex-layout.col#suggestedCarouselOutOfStock": {
    "title": "[Col] - [MID] - Suggested Carousel oos",
    "props": {
      "blockClass": "suggestedCarousel"
    },
    "children": [
      "shelf.relatedProducts#suggestedCarousel",
      "link.product#linkToPlpOutOfStock"
    ]
  },
  "shelf.relatedProducts#suggestedCarousel": {
    "blocks": [
      "product-summary.shelf.product-comparison#bauknechtPLP-carousel-no-comparison-button"
    ],
    "props": {
      "recommendation": "suggestions",
      "productList": {
        "maxItems": 10,
        // "itemsPerPage": 5,
        "itemsPerPage": 3,
        "minItemsPerPage": 1,
        "scroll": "BY_PAGE",
        "arrows": true,
        "titleText": "Lassen Sie sich von anderen Modellen inspirieren"
      },
      "blockClass": [
        "margin3125",
        "slideMargin",
        "suggestions",
        "suggestedCarousel"
      ] //"slide20width" removed
    }
  },
  "link.product#linkToPlpOutOfStock": {
    "title": "[Link] - Link to PLP",
    "props": {
      "blockClass": "linkToPlp",
      "label": "Aktuelle Produkte entdecken",
      "href": "/hausgeraete/{category1}/{category2}",
      "escapeLinkRegex": " [&]",
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "large"
      }
    }
  },
  "link.product#linkToPlpUnsellable": {
    "title": "[Link] - Link to PLP",
    "props": {
      "blockClass": ["linkToPlp", "linkToPlpUnsellable"],
      "label": "Aktuelle Produkte entdecken",
      "href": "/hausgeraete/{category1}/{category2}",
      "escapeLinkRegex": " [&]",
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "large"
      }
    }
  },
  "flex-layout.row#suggestedCarouselUnsellable": {
    "title": "[Row] - [MID] - Suggested Carousel unsellable",
    "props": {
      "blockClass": "suggestedCarousel"
    },
    "children": ["flex-layout.col#suggestedCarouselUnsellable"]
