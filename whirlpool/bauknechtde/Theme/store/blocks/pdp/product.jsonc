  "rich-text#warrantyModalContent2Title": {
    "title": "[TEXT] - [MID] - Modal Content 2 Title",
    "props": {
      "text": "### Sorglos Plus",
      "blockClass": ["modalContentTitle", "modalContentTitleWarranty"]
    }
