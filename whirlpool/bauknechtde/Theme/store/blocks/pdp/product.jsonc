    "children": [
      "rich-text#warrantyModalContent2Title",
      "rich-text#warrantyModalContent2TextOverList",
      "flex-layout.row#listContainerRow",
      "rich-text#warrantyModalContent2TextUnderList",
      "rich-text#warrantyModalContent2UnderlinedText"
    ],
