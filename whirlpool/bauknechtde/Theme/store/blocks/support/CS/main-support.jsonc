  //PIANI DI PROTEZIONE
  "flex-layout.row#piani-di-protezione": {
    "children": ["flex-layout.col#colPiani-di-protezione"]
  },
  "flex-layout.col#colPiani-di-protezione": {
    "children": [
      "rich-text#anchor-piani-di-protezione",
      "flex-layout.row#halfBanner-piani-di-protezione"
    ]
  },

  "rich-text#anchor-piani-di-protezione": {
    "title": "[ANCHOR] - MID - Anchor Piani di Protezione",
    "props": {
      "htmlId": "halfBanner1",
      "text": ""
    }
  },

  "flex-layout.row#halfBanner-piani-di-protezione": {
    "title": "[ROW] - MID - Row Half Banner Piani di Protezione",
    "props": {
      "preventHorizontalStretch": false,
      "blockClass": [
        "columnPostillaBannerAntonio",
        "bgColorAntonioHero",
        "bannerSupporto"
      ]
    },
    "children": [
      "flex-layout.col#mainSupportRightPartHalfBanner",
      "flex-layout.col#mainSupportLeftPartHalfBanner"
    ]
  },
  "flex-layout.col#mainSupportRightPartHalfBanner": {
    "title": "[COL] - MID - Column Right Part Half Banner Piani Di Protezione",
    "props": {
      "width": "50%",
      "blockClass": "columnPostillaImage"
    },
    "children": ["image#halfBannerImagePianiDiProtezione"]
  },
  "image#halfBannerImagePianiDiProtezione": {
    "title": "[IMAGE] - MID - Image Half Banner Image Piani Di Protezione",
    "props": {
      "src": "assets/ph-square.jpg",
      "blockClass": "imagePostillaBanner"
    }
  },
  "flex-layout.col#mainSupportLeftPartHalfBanner": {
    "title": "[COL] - MID - Column Left Part Half Banner Piani Di Protezione",
    "props": {
      "width": "50%",
      "preventVerticalStretch": true,
      "blockClass": ["columnPostillaPromo", "columnPostillaBottom"]
    },
    "children": [
      "rich-text#main-supportHalfBannerTitle",
      "rich-text#main-supportHalfBannDesc",
      "experimental__visibility-layout#main-supportHalfBannerButton",
      "experimental__visibility-layout#main-supportHalfBannerButton2",
      "rich-text#main-supportHalfBannerPostilla"
    ]
  },
  "rich-text#main-supportHalfBannerTitle": {
    "title": "[TEXT] - TOP - Main Support Half Banner Title",
    "props": {
      "text": "# Frigorifero combinato 4 porte",
      "blockClass": "titleHBannerPostillaPromo"
    }
  },
  "rich-text#main-supportHalfBannDesc": {
    "title": "[TEXT] - TOP - Main Support Half Banner Title Description",
    "props": {
      "text": "33% di spazio extra per frutta e verdura *",
      "blockClass": ["descBannerPostillaPromo"]
    }
  },
  "experimental__visibility-layout#main-supportHalfBannerButton": {
    "title": "[EXPERIMENTAL-VISIBILITY] - TOP - Main Support Half Banner First button",
    "props": {
      "visible": true
    },
    "children": ["link#main-supportHalfBannerButton"]
  },
  "experimental__visibility-layout#main-supportHalfBannerButton2": {
    "title": "[EXPERIMENTAL-VISIBILITY] - TOP - Main Support Half Banner Second button",
    "props": {
      "visible": true
    },
    "children": ["link#main-supportHalfBannerButton2"]
  },
  "link#main-supportHalfBannerButton": {
    "title": "[LINK] - TOP - Slide button",
    "props": {
      "label": "CLICCA QUI",
      "href": "#",
      "displayMode": "button",
      "blockClass": ["postillaButton", "homeSliderButton"],
