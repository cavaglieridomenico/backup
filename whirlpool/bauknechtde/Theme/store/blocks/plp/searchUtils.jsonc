{
  "search-result-layout#dep": {
    "blocks": [
      "search-result-layout.desktop#dep",
      "search-result-layout.mobile#Dep",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop#dep": {
    "children": [
      "experimental__visibility-layout#bannerPLP",
      "flex-layout.row#searchtitleDep",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#productsDep",
      "flex-layout.row#fetchmore",
      "flex-layout.row#space"
    ],
    "props": {
      "pagination": "show-more",
      "preventRouteChange": true,
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      }
    }
  },
  // "experimental__visibility-layout#heroBannerPLP": {
  //   "props": {
  //     "visible": true
  //   },
  //   "children": ["info-card#heroBannerSearch"]
  // },
  "experimental__visibility-layout#heroBannerPLPCollezioneIncasso": {
    "props": {
      "visible": true
    },
    "children": ["info-card#heroBannerCollezioneIncasso"]
  },
  "experimental__visibility-layout#CarouselPLP": {
    "props": {
      "visible": false,
      "blockClass": "plpSlider"
    },
    "children": ["slider-layout#plp"]
  },
  "slider-layout#plp": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "autoplay": {
        "timeout": 5000,
        "stopOnHover": false
      },
      "blockClass": "carousel"
    },
    "children": [
      "experimental__visibility-layout#bannerSlider1",
      "experimental__visibility-layout#bannerSlider2",
      "experimental__visibility-layout#bannerSlider3"
    ]
  },
  "info-card#heroBannerCollezioneIncasso": {
    "props": {
      "id": "hero-banner-main-search",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/ph-square.jpg",
      "headline": "Supreme Care",
      "callToActionMode": "CallToActionEnum",
      "callToActionEnum": "none",
      "blockClass": [
        "heroBanner",
        "heroBannerSearch",
        "heroBannerCollezioneIncasso"
      ],
      "textAlignment": "left"
    }
  },
  "flex-layout.row#searchtitleDep": {
    "children": ["rich-text#category"]
  },

  "search-result-layout#generic": {
    "blocks": [
      "search-result-layout.desktop.product-comparison#searchGeneric",
      "search-result-layout.mobile#Generic",
      "search-not-found-layout"
    ]
  },
  "search-result-layout.desktop.product-comparison#searchGeneric": {
    "children": [
      "flex-layout.row#queryRow",
      // "flex-layout.row#searchinfo",
      "flex-layout.row#fetchprevious",
      "flex-layout.row#products",
      "flex-layout.row#fetchmore",
      "flex-layout.row#space",
      "product-comparison-drawer"
    ]
  },
  "flex-layout.row#queryRow": {
    "children": ["rich-text#query", "search-title.v2"],
    "props": {
      "blockClass": "queryRow"
    }
  },
  "rich-text#query": {
    "props": {
      "text": "Ricerca: ",
      "blockClass": "query"
    }
  },
  "search-result-layout.mobile#Generic": {
    "children": [
      "flex-layout.row#queryRow",
      "flex-layout.row#countAndorderByMobile",
      "flex-layout.row#breadcrumbsPLPCustom",
      // "flex-layout.row#fetchprevious",
      "flex-layout.row#contentmobile",
      "flex-layout.row#fetchmore",
      // "flex-layout.row#space",
      "experimental__visibility-layout#bannerBottomPLP",
      "experimental__visibility-layout#bannerBottomPLP2",
      "experimental__visibility-layout#bannerBottomPLP3",
      "product-comparison-drawer"
    ]
  },
  "flex-layout.row#countAndorderByMobile": {
    "children": ["search-products-count-per-page", "order-by.v2"],
    "props": {
      "blockClass": ["orderByMobileCol", "disFlexDirCol", "widthFit"]
    }
  },
  "search-result-layout.desktop": {
    "children": [
      "flex-layout.row#breadcrumbsPLPCustom",
      "experimental__visibility-layout#bannerPLP",
      "flex-layout.row#searchtitle",
      "flex-layout.row#result"
    ],
    "props": {
      "pagination": "show-more",
      "preventRouteChange": true,
      "mobileLayout": {
        "mode1": "small",
        "mode2": "normal"
      }
    }
  },
  "link#home": {
    "props": {
      "href": "/",
      "label": "Home",
      "blockClass": "breadLink"
    }
  },
  "image#arrowBread": {
    "props": {
      "src": "assets/icons/arrowIconBread.svg"
    }
  },
  "image#arrowBreadDown": {
    "props": {
      "src": "assets/icons/angle-down.png"
    }
  },
  "image#arrowBreadUp": {
    "props": {
      "src": "assets/icons/angle-up.png"
    }
  },
  "link#dep": {
    "props": {
      "href": "#",
      "label": "DEPARTMENT",
      "blockClass": ["breadLink", "dep"]
    }
  },
  "link#cat": {
    "props": {
      "href": "/dep/cat",
      "label": "CATEGORY",
      "blockClass": "breadLink"
    }
  },

