  },
  "flex-layout.row#firstRowAdvantageForYou3": {
    "props": {
      "blockClass": "firstRowAdvantageForYou"
    },
    "children": [
      "image#firstImageAdvantageForYou",
      "rich-text#firstTextAdvantageForYou"
    ]
