{
  "memory": 256,
  "ttl": 10,
  "timeout": 120,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "events": {
    "orderCreated": {
      "sender": "vtex.orders-broadcast",
      "topics": ["order-created"]
    }
  },
  "routes":{
    "redirectToDnG": {
      "path": "/app/dng/redirect/:orderId/:itemId/:warrantyId",
      "public": true
    },
    "sendOrGetOrderData": {
      "path": "/app/dng/order/:orderId",
      "public": true
    }
  }
}
