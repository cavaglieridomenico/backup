{
  "store.custom#store-locator": {
    "blocks": [
      // "analytics#store-locator",
      "flex-layout.row#store-locator-title",
      "flex-layout.row#store-locator-app"
    ]
  },
  /* ---------------------- TITLE ROW  ---------------------------- */
  "flex-layout.row#store-locator-title": {
    "props": {
      "blockClass": "store-locator-title-style"
    },
    "children": ["rich-text#store-locator-text"]
  },
  "rich-text#store-locator-text": {
    "props": {
      "blockClass": "store-locator-title-style",
      "text": "Trouver un revendeur"
    }
  },
  /* ---------------------- STORE LOCATOR APP  ---------------------------- */
  "flex-layout.row#store-locator-app": {
    "props": {
      "blockClass": "store-locator-container"
    },
    "children": ["flex-layout.col#store-locator-app"]
  },
  "flex-layout.col#store-locator-app": {
    "children": ["store-locator-app"]
  },
  "store-locator-app": {}
}
