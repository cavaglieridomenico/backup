  "flex-layout.row#follow-us-row-mobile": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "followUsRowMobile"
    },
    "children": ["rich-text#follow-us-text-mobile", "social-networks#mobile"]
