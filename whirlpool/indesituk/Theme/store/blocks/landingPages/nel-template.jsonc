{
    "store.custom#nel-template":{
      "blocks": [
        "responsive-layout.desktop#nel",
        "responsive-layout.mobile#nel-mobile"
      ]
    },
    /* ---MAXI CONTAINER--- */
    "responsive-layout.desktop#nel": {
      "children": [
        "flex-layout.row#nel-maxi"
      ]
    },
    "flex-layout.row#nel-maxi": {
      "children": [
        "flex-layout.col#nel-maxi"
      ],
      "props": {
        "blockClass": "breadcrumb-container-landing-template-desktop"
      }
    },
    "flex-layout.col#nel-maxi": {
      "children": [
        "breadcrumb#nel-desktop",
        "flex-layout.row#nel-intro-desktop",
        "flex-layout.row#text-after-intro-nel-desktop",
        "flex-layout.row#energy-efficiency-desktop",
        "flex-layout.row#text-after-energy-efficency-desktop",
        "flex-layout.row#energy-label-washing-machines-desktop",
        "learn-more-NEL-little-cards-desk#nel-template",
        "flex-layout.row#energy-label-washer-dryers-desktop",
        /* QUI CI VA LA CUSTOM APP DEL BOTTONE READ MORE READ LESS */
        "flex-layout.row#first-static-button-discover-more-ecodesign-desktop-container",
        "flex-layout.row#energy-label-dishwashers-desktop",
        /* QUI CI VA LA CUSTOM APP DEL BOTTONE READ MORE READ LESS */
