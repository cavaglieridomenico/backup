{
  /* ---------------------- FOOTER ---------------------------- */
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  /* ---------------------- DESKTOP ---------------------------- */
  "footer-layout.desktop": {
    "props": {
      "blockClass": "footer-container-desktop"
    },
    "children": ["flex-layout.row#footer-row"]
  },
  /* ---------------------- FLEX LAYOUT ---------------------------- */
  "flex-layout.row#footer-row": {
    "children": ["vtex.menu@2.x:menu#footer-menu", "flex-layout.col#follow-us"],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "footerRowDesktop"
    }
  },
  "flex-layout.col#follow-us": {
    "props": {
      "blockClass": "follow-us-container"
    },
    "children": ["flex-layout.row#follow-us-row"]
  },
  /* ---------------------- MENU ITEM LABELS ---------------------------- */
  "vtex.menu@2.x:menu#footer-menu": {
    "props": {
      "blockClass": "footerMenu"
    },
    "children": [
      //"menu-item#store-locator",
      //"menu-item#dolt-together",
      /*  "menu-item#faq-footer", */
      //"menu-item#support",
      //"menu-item#legal-issues",
      //"menu-item#safety-notices"
      "link#footerMenu_01-item_01",
      "link#footerMenu_01-item_02",
      "link#footerMenu_01-item_03",
      "link#footerMenu_01-item_04",
      "link#footerMenu_01-item_05",
      "link#footerMenu_01-item_06"
    ]
  },
  /* ---------------------- FOLLOW US ROW ---------------------------- */
  "flex-layout.row#follow-us-row": {
    "props": {
      "preventHorizontalStretch": true
    },
    "children": ["rich-text#follow-us-text", "social-networks"]
  },
  /* ---------------------- FOLLOW US ---------------------------- */
  "rich-text#follow-us-text": {
    "props": {
      "text": "Follow us",
      "blockClass": "followUsText"
    }
  },
  /* ---------------------- SOCIAL NETWORKS ---------------------------- */
  "social-networks": {
    "props": {
      "blockClass": "social-icons",
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/IndesitUK/",
          "target": "_blank"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/indesituk/",
          "target": "_blank"
        },
        {
          "name": "Twitter",
          "url": "https://twitter.com/IndesitUK",
          "target": "_blank"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/channel/UCM7lOO-oHvxmET-aQ7zZ_nA",
          "target": "_blank"
        }
      ]
    }
  },
  /* ---------------------- MOBILE ---------------------------- */ /* BREAK POINT DA 1025 PX */
  "footer-layout.mobile": {
    "props": {
      "blockClass": "footer-container-mobile"
    },
    "children": ["flex-layout.row#footer-row-mobile"]
  },
  /* ---------------------- FLEX LAYOUT ---------------------------- */
  "flex-layout.row#footer-row-mobile": {
    "children": [
      "flex-layout.col#links-mobile",
      "flex-layout.col#follow-us-mobile"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "footerRowMobile"
    }
  },
  "flex-layout.col#links-mobile": {
    "props": {
      "blockClass": "links-footer"
    },
    "children": ["flex-layout.row#links-row-mobile"]
  },
  "flex-layout.col#follow-us-mobile": {
    "props": {
      "blockClass": "follow-us-footer"
    },
    "children": ["flex-layout.row#follow-us-row-mobile"]
  },
  /* ---------------------- LINKS ROW ---------------------------- */
  "flex-layout.row#links-row-mobile": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "links"
    },
    "children": ["flex-layout.col#links-left", "flex-layout.col#links-right"]
  },
  "flex-layout.col#links-left": {
    "props": {
      "blockClass": "links-left"
    },
    "children": [
      "link#footerMenu-mobile-item_01",
      "link#footerMenu-mobile-item_02",
      "link#footerMenu-mobile-item_03"
    ]
  },
  "flex-layout.col#links-right": {
    "props": {
      "blockClass": "links-right"
    },
    "children": [
      /* "link#faq-mobile", */
      //"link#legal-issues",
      //"link#safety-notices"
      "link#footerMenu-mobile-item_04",
      "link#footerMenu-mobile-item_05",
      "link#footerMenu-mobile-item_06"
    ]
  },
  /* ---------------------- LINKS LEFT ---------------------------- */
  "link#footerMenu-mobile-item_01": {
    "props": {
      "label": "Store Locator",
      "href": "/store-locator",
      "isAnalyticsEvent": true,
      "analyticsEventName": "menu_footer_click",
      "analyticsType": "",
      "isPrevent": false,
      "clickArea": "footer",
      "blockClass": ["footer-item"]
    }
    //"children": ["rich-text#sl-text"]
  },
  // "rich-text#sl-text": {
  //   "props": {
  //     "blockClass": "links-text",
  //     "text": "Store Locator"
  //   }
  // },
  "link#footerMenu-mobile-item_02": {
    "props": {
      "label": "Support",
      "href": "https://www.indesitservice.co.uk/",
      "target": "_blank",
      "isAnalyticsEvent": true,
      "analyticsEventName": "menu_footer_click",
      "analyticsType": "",
      "isPrevent": false,
      "clickArea": "footer",
      "blockClass": ["footer-item"]
    }
    //"children": ["rich-text#sp-text"]
  },
  "link#footerMenu-mobile-item_03": {
    "props": {
      "label": "Cookie Policy",
      "href": "/legal-issues/cookies-disclaimer",
      "isAnalyticsEvent": true,
      "analyticsEventName": "menu_footer_click",
      "analyticsType": "",
      "isPrevent": false,
      "clickArea": "footer",
      "blockClass": ["footer-item"]
    }
    //"children": ["rich-text#sp-text"]
  },
