{
  "store.custom#discontinued-products": {
    "blocks": [
      "product-meta-handler",
      "responsive-layout.mobile#discontinued-products",
      "responsive-layout.desktop#discontinued-products",
      "flex-layout.row#newsletter-row"
    ]
  },
  "product-meta-handler": {},
  "rich-text#discontinued-products": {
    "props": {
      "text": "Discontinued-products",
      "textPosition": "CENTER"
    }
  },
  "discontinued-products": {
    "children": [
      "flex-layout.row#product-name"
    ]
  },
  "responsive-layout.desktop#discontinued-products": {
    "children": [
      "discontinued-products#desktop"
    ]
  },
  "responsive-layout.mobile#discontinued-products": {
    "children": [
      "discontinued-products#mobile"
    ]
  },
  "discontinued-products#desktop": {
    "children": [
      "flex-layout.row#product-breadcrumb",
      "flex-layout.row#product-name",
      "flex-layout.row#product-main-Discontinued",
      "condition-layout.product#bannerPromoWithBadge1",
      "condition-layout.product#bannerPromoWithBadge3",
      "condition-layout.product#bannerPromoFairy",
      "condition-layout.product#isAccessorioIntestazioneTab",
      "condition-layout.product#isAccessorioTab",
      "condition-layout.product#similars",
      "condition-layout.product#suggestions",
      "condition-layout.product#showtogether",
      "product-comparison-drawer"
    ]
  },
  "flex-layout.row#product-main-Discontinued": {
    "props": {
      "colGap": 7,
      "rowGap": 7,
      "marginTop": 4,
      "marginBottom": 7,
      "paddingTop": 7,
      "paddingBottom": 7,
      "blockClass": [
        "width1280Center",
        "mainRowPDP"
      ],
      "preserveLayoutOnMobile": false
    },
    "children": [
      "flex-layout.col#product-image-disc",
      "flex-layout.col#right-col-discontinued"
    ]
  },
  "flex-layout.col#product-image-disc": {
    "children": [
      "video-player-thron-pdp"
    ]
  },
  "flex-layout.col#right-col-discontinued": {
    "props": {
      "blockClass": "product-desc-info",
      "preventVerticalStretch": true,
      "rowGap": 0
    },
    "children": [
      "condition-layout.product#discontinued",
      "flex-layout.row#productPreServizi-discontinued",
      "condition-layout.product#servizi",
      "flex-layout.row#halfSpace",
      "condition-layout.product#message",
      "condition-layout.product#isAccessoriosku-selector",
      "condition-layout.product#isAccessoriominimal",
      "condition-layout.product#showPricePDPSaving",
      "condition-layout.product#showPricePDPSelling",
      "condition-layout.product#ivaPDP",
      "condition-layout.product#isAvailableButton",
      "flex-layout.col#partToShow"
    ]
  },
  "flex-layout.row#productPreServizi-discontinued": {
    "props": {
      "blockClass": "preServizi"
    },
    "children": [
      "condition-layout.product#etichetta-image-pre-discontinued",
      "condition-layout.product#rating"
    ]
  },
  "condition-layout.product#etichetta-image-pre-discontinued": {
    "title": "[Condition Layout] - Energy label Discontinued",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "energy-label"
          }
        }
      ],
      "Then": "flex-layout.row#old-label-pre-discontinued",
      "Else": "flex-layout.row#new-label-pre-discontinued"
    }
  },
  "flex-layout.row#new-label-pre-discontinued": {
    "children": [
      "image-from-specification#EtichettaEnergeticaNew-pre-discontinued"
    ]
  },
  "flex-layout.row#old-label-pre-discontinued": {
    "children": [
      "image-from-specification#EtichettaEnergetica-pre-discontinued"
    ]
  },
  "image-from-specification#EtichettaEnergeticaNew-pre-discontinued": {
    "props": {
      "specificationName": "EnergyLogo_image",
      "imageLink": "Specification.new-energy-label",
      "labelWithHref": true,
      "target": "_blank"
    }
  },
  "image-from-specification#EtichettaEnergetica-pre-discontinued": {
    "props": {
      "specificationName": "EnergyLogo_image",
      "imageLink": "Specification.energy-label",
      "labelWithHref": true,
      "target": "_blank"
    }
  },
  "discontinued-products#mobile": {
    "children": [
      "flex-layout.row#product-main-mobile",
      "condition-layout.product#isAccessorioTab"
    ]
  },
  "condition-layout.product#isAvailableStickyMobile-discontinued": {
    "title": "[Condition Layout] - Sticky Discontinued",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "DISPONIBILITÃ€",
            "value": "Whirlpool Online Shop"
          }
        }
      ],
      "Then": "condition-layout.product#sticky-discontinuedMobile-discontinued"
    }
  },
  "condition-layout.product#sticky-discontinuedMobile-discontinued": {
    "title": "[Condition Layout] - Promo Badge Collection",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#sticky-discontinued-with-priceMobile-discontinued"
    }
  },
  "condition-layout.product#sticky-discontinued-with-priceMobile-discontinued": {
    "title": "[Condition Layout] - Sticky discontinued with price ",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#pdpSticky"
    }
  },
  "flex-layout.col#single-col-discontinued": {
    "props": {
      "blockClass": "product-desc-info",
      "preventVerticalStretch": true,
      "rowGap": 0
    },
    "children": [
      "condition-layout.product#discontinued",
      "flex-layout.row#productTypePDP",
      "stack-layout#imagesPDP",
      "flex-layout.row#product-name",
      "flex-layout.row#specificationsRowMobile",
      "flex-layout.row#productPreServiziMobile",
      "condition-layout.product#isAvailableButton",
      "condition-layout.product#servizi",
      "condition-layout.product#premium-banner",
      "condition-layout.product#isAccessoriosku-selector",
      "condition-layout.product#isAccessoriominimal"
    ]
  }
}