{
  "name": "recipes-graphql",
  "vendor": "plwhirlpoolqa",
  "version": "0.1.2",
  "title": "Recipes graphql",
  "description": "GraphQL app that allow to get all the information about recipes stored in the masterdata",
  "builders": {
    "docs": "0.x",
    "node": "6.x",
    "graphql": "1.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    }
  ],
  "dependencies": {},
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
