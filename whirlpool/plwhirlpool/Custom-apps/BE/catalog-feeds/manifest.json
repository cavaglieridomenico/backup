{
  "name": "catalog-feed",
  "vendor": "plwhirlpool",
  "version": "0.0.1",
  "title": "Catalog feed builder",
  "description": "Catalog Feed Builder",
  "mustUpdateAt": "2022-11-14",
  "categories": [],
  "dependencies": {
    "vtex.search-resolver": "1.x",
    "vtex.search-graphql": "0.x"
  },
  "builders": {
    "node": "6.x",
    "docs": "0.x"
  },
  "scripts": {
    "prereleasy": "bash lint.sh"
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "LogisticsAdmin"
    },
    {
      "name": "read_prices"
    },
    {
      "name": "OMSViewer"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "vtex.search-resolver:resolve-graphql"
    }
  ],
  "settingsSchema": {
    "title": "Catalog Feed Builder",
    "type": "object",
    "properties": {
      "collectionId": {
        "title": "Collection ID",
        "type": "string"
      },
      "publicUrl": {
        "title": "Public site URL",
        "type": "string",
        "default": "https://www.bauknecht.de"
      },
      "shippingCost": {
        "title": "Shipping Cost",
        "type": "number",
        "default": 0
      },
      "shippingTime": {
        "title": "Shipping Time",
        "type": "string",
        "default": 6
      },
      "cachePeriod": {
        "title": "Cache time (minutes)",
        "type": "number",
        "default": 60
      }
    }
  },
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
