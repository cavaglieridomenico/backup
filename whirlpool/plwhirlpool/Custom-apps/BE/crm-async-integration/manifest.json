  "settingsSchema": {
    "title": "CRM connector",
    "type": "object",
    "properties": {
      "server": {
        "title": "CRM or SAP-PO",
        "type": "string",
        "enum": [
          "crm",
          "sap-po"
        ]
      },
      "crmEnvironment": {
        "title": "CRM environment",
        "type": "string",
        "enum": [
          "production",
          "quality"
        ]
      },
      "crmPassword": {
        "title": "CRM password",
        "type": "string"
      },
      "gcpHost": {
        "title": "GCP host",
        "type": "string"
      },
      "gcpProjectId": {
        "title": "GCP project id",
        "type": "string"
      },
      "gcpClientEmail": {
        "title": "GCP client email",
        "type": "string"
      },
      "gcpPrivateKey": {
        "title": "GCP private key",
        "type": "string"
      },
      "gcpTargetAudience": {
        "title": "GCP target audience",
        "type": "string"
      },
      "enabledAPICredentials": {
        "title": "Enabled API credentials",
        "type": "string"
      },
      "enabledMDKey": {
        "title": "Enabled MD key",
        "type": "string"
      },
      "authCookie": {
        "title": "Authentication cookie",
        "type": "string"
      }
    }
  },
