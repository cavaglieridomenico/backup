{
  "version": "0.0.1",
  "name": "new-relic-app",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "lint:locales": "intl-equalizer",
    "test": "cd ./react && yarn test"
  },
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "messages"
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "messages/*.json": [
      "yarn lint:locales"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@vtex/intl-equalizer": "^2.5.0",
    "@vtex/prettier-config": "^0.3.5",
    "@vtex/slugify": "^1.0.3",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "2.3.2",
    "react-full-page": "^0.1.11",
    "typescript": "^4.3.2",
    "vtex.search-page-context": "http://vtex.vtexassets.com/_v/public/typings/v1/vtex.search-page-context@0.2.0/public/@types/vtex.search-page-context"
  },
  "dependencies": {
    "@types/react-helmet": "^6.1.1",
    "classnames": "^2.3.1",
    "helmet": "^4.6.0",
    "react-device-detect": "^1.17.0",
    "react-ga": "^3.3.0",
    "react-player": "^2.9.0",
    "react-responsive-carousel": "^3.2.19"
  }
}
