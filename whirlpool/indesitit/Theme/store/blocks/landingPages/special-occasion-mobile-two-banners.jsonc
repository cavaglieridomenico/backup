//MOBILE LAYOUT
{
  "responsive-layout.mobile#special-occasion-two-banners-mobile": {
    "props": {
      "blockClass": "special-occasion-two-banners-mobile-style"
    },
    "children": [
      "flex-layout.row#special-occasion-two-banners-mobile-maxi-container"
    ]
  },
  "flex-layout.row#special-occasion-two-banners-mobile-maxi-container": {
    "children": [
      "flex-layout.col#special-occasion-two-banners-mobile-maxi-container"
    ],
    "props": {
      "blockClass": "special-occasion-two-banners-mobile-maxi-container"
    }
  },
  "flex-layout.col#special-occasion-two-banners-mobile-maxi-container": {
    "children": [
      //"analytics#landing-pages",
      /* "breadcrumb#special-occasion-two-banners-mobile", */
      "breadcrumb-custom#special-occasion-two-banners-mobile",
      "flex-layout.row#special-occasion-two-banners-intro-mobile",
      "flex-layout.row#special-occasion-two-banners-product-detail-card-05-mobile"
    ],
    "props": {
      "blockClass": "special-occasion-two-banners-mobile-maxi-container-style"
    }
  },
  "breadcrumb#special-occasion-two-banners-mobile": {
    "props": {
      "categories": ["/Home/", "special-occasion-two-banners"],
      "showOnMobile": true,
      "blockClass": "breadcrumb-landing-template-mobile"
    }
  },
  "breadcrumb-custom#special-occasion-two-banners-mobile": {
    "props": {
      /* "blockClass": "containerBreadcrumbs", */
      "firstPage": "Home",
      "currentPage": "special-occasion-two-banners"
    }
  },
  /* INTRO TO PRODUCT MOBILE*/
  "flex-layout.row#special-occasion-two-banners-intro-mobile": {
    "children": [
      "flex-layout.col#special-occasion-two-banners-intro-image-mobile",
      "flex-layout.col#special-occasion-two-banners-intro-desc-mobile"
    ],
    "props": {
      "blockClass": "text-special-occasion-two-banners-intro-mobile"
    }
  },
  "flex-layout.col#special-occasion-two-banners-intro-image-mobile": {
    "children": ["image#special-occasion-two-banners-intro-mobile"],
    "props": {
      "blockClass": "intro-image-mobile"
    }
  },
  "image#special-occasion-two-banners-intro-mobile": {
    "props": {
      "blockClass": "product-launch-intro-mobile",
      "src": "https://i.picsum.photos/id/1002/4312/2868.jpg?hmac=5LlLE-NY9oMnmIQp7ms6IfdvSUQOzP_O3DPMWmyNxwo"
    }
  },
  "flex-layout.col#special-occasion-two-banners-intro-desc-mobile": {
    "children": [
      "rich-text#special-occasion-two-banners-intro-desc-title-mobile",
      "rich-text#special-occasion-two-banners-intro-desc-paragraph-mobile",
      "rich-text#special-occasion-two-banners-intro-button-go-to-product-mobile"
    ],
    "props": {
      "blockClass": "intro-desc-mobile"
    }
  },
  "rich-text#special-occasion-two-banners-intro-button-go-to-product-mobile": {
    "props": {
      "text": "[ GO TO PRODUCT/LINE ](/)",
      "blockClass": ["button-intro-go-to-product-mobile"]
    }
  },
  "rich-text#special-occasion-two-banners-intro-desc-title-mobile": {
    "props": {
      "text": "Product or product line name",
      "blockClass": "intro-landing-template-title-mobile"
    }
  },
  "rich-text#special-occasion-two-banners-intro-desc-paragraph-mobile": {
    "props": {
      "text": "Payoff to intruduce the products presented, to promote the product in it's context",
      "blockClass": "intro-landing-template-paragraph-mobile"
    }
  },
  /* PRODUCT DETAIL CARD WITH TEXT 05 MOBILE */
  "flex-layout.row#special-occasion-two-banners-product-detail-card-05-mobile": {
    "children": [
      "flex-layout.col#special-occasion-two-banners-product-detail-card-05-mobile"
    ],
    "props": {
      "blockClass": "special-occasion-two-banners-product-detail-card-05-mobile"
    }
  },
  "flex-layout.col#special-occasion-two-banners-product-detail-card-05-mobile": {
    "children": [
      "flex-layout.row#special-occasion-two-banners-product-detail-card-05-container-mobile"
    ]
  },
  "flex-layout.row#special-occasion-two-banners-product-detail-card-05-container-mobile": {
    "children": [
      "flex-layout.col#special-occasion-two-banners-product-detail-card-05-image-mobile",
      "flex-layout.col#special-occasion-two-banners-product-detail-card-05-container-desc-mobile"
    ],
    "props": {
      "blockClass": "container-global-bordered-text-image-landing-template-mobile"
    }
  },
  /* ------------------------------------ */
  "flex-layout.col#special-occasion-two-banners-product-detail-card-05-image-mobile": {
    "children": [
      /* "is-image-or-video-mobile#special-occasion-two-banners-mobile" */
    ],
    "props": {
      "blockClass": "container-image-bordered-text-image-landing-template-mobile"
    }
  },
