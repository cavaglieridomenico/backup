  "responsive-layout.desktop#trouble-shooting": {
    "children": ["flex-layout.row#trouble-shooting-container-maxi-desktop"]
  },
  "flex-layout.row#trouble-shooting-container-maxi-desktop": {
    "children": ["flex-layout.col#trouble-shooting-container-maxi-desktop"],
    "props": {
      "blockClass": "trouble-shooting-container-maxi-desktop"
    }
  },
  /* MAXI CONTAINER OF ALL ELEMENT IN THE PAGE */
  "flex-layout.col#trouble-shooting-container-maxi-desktop": {
    "children": [
      "breadcrumb-custom#trouble-shooting",
      "flex-layout.col#rich-text-desktop",
      "trouble-shooting-page#desktop"
    ],
    "props": {
      "blockClass": "trouble-shooting-container-maxi-desktop"
    }
  },
  "no-index": {},
  "custom-helmet": {},
  /* BREADCRUMB */
  "breadcrumb-custom#trouble-shooting": {
    "props": {
      /* "blockClass": "containerBreadcrumbs", */
      "firstPage": "Home",
      "previousPage": "Assistenza",
      "previousPath": "/assistenza",
      "currentPage": "Problemi e soluzioni"
    }
  },
  "flex-layout.col#rich-text-desktop": {
    "children": [
      "rich-text#trouble-shooting-title-desktop",
      "rich-text#trouble-shooting-desc-desktop"
    ],
    "props": {
      "blockClass": "rich-text-container"
    }
  },
  "rich-text#trouble-shooting-title-desktop": {
    "props": {
      "text": "Qui potrai trovare i nostri suggerimenti per risolvere i problemi del tuo elettrodomestico.",
      "blockClass": "ts-title-desktop"
    }
  },
  "rich-text#trouble-shooting-desc-desktop": {
    "props": {
      "text": "In pochi click tuttle le soluzioni ai semplici problemi di funzionamento e i suggerimenti per la manutenzione di lavatrici, lavastoviglie, frigoriferi, forni e piani cottura. Ti aiutiamo a riparare il tuo elettrodomestico Indesit con l'assistenza tecnica online.",
      "blockClass": "ts-desc-desktop"
    }
  },
  "trouble-shooting-page#desktop": {}
}
