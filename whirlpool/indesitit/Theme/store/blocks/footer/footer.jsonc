{
  /* ---------------------- FOOTER ---------------------------- */
  "footer": {
    "blocks": [
      "footer-layout.desktop",
      "footer-layout.mobile"
    ]
  },
  /* ---------------------- DESKTOP ---------------------------- */
  "footer-layout.desktop": {
    "props": {
      "blockClass": "footer-container-desktop"
    },
    "children": [
      "flex-layout.row#footer-row"
    ]
  },
  /* ---------------------- FLEX LAYOUT ---------------------------- */
  "flex-layout.row#footer-row": {
    "children": [
      "vtex.menu@2.x:menu#footer-menu",
      "flex-layout.col#follow-us"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "footerRowDesktop"
    }
  },
  "flex-layout.col#follow-us": {
    "props": {
      "blockClass": "follow-us-container"
    },
    "children": [
      "flex-layout.row#follow-us-row"
    ]
  },
  /* ---------------------- MENU ITEM LABELS ---------------------------- */
  "vtex.menu@2.x:menu#footer-menu": {
    "props": {
      "blockClass": "footerMenu"
    },
    "children": [
      /* "menu-item#store-locator", */
      // "menu-item#dolt-together",
      /*  "menu-item#faq-footer", */
      // "menu-item#support",
      // "menu-item#legal-issues",
      // "menu-item#manuals",
      // "menu-item#safety",
      "link#footerMenu_01-item_01",
      "link#footerMenu_01-item_02",
      "link#footerMenu_01-item_03",
      "link#footerMenu_01-item_04",
      "link#footerMenu_01-item_05",
      "link#footerMenu_01-item_youreko",
      "toggle-display#preferences-cookies"
    ]
  },
  "toggle-display#preferences-cookies": {
    "props": {
      "text": "Gestione preferenze"
    }
  },
  /* ---------------------- FOLLOW US ROW ---------------------------- */
  "flex-layout.row#follow-us-row": {
    "props": {
      "preventHorizontalStretch": true
    },
    "children": [
      "rich-text#follow-us-text",
      "social-networks"
    ]
  },
  /* ---------------------- FOLLOW US ---------------------------- */
  "rich-text#follow-us-text": {
    "props": {
      "text": "Seguici",
      "blockClass": "followUsText"
    }
  },
  /* ---------------------- SOCIAL NETWORKS ---------------------------- */
  "social-networks": {
    "props": {
      "blockClass": "social-icons",
      "socialNetworks": [
        {
          "name": "Facebook",
          "url": "https://www.facebook.com/Indesit/",
          "target": "_blank"
        },
        {
          "name": "Instagram",
          "url": "https://www.instagram.com/indesit_italia/",
          "target": "_blank"
        },
        {
          "name": "Youtube",
          "url": "https://www.youtube.com/c/IndesitItalia",
          "target": "_blank"
        }
      ]
    }
  },
  /* ---------------------- MOBILE ---------------------------- */ /* BREAK POINT DA 1025 PX */
  "footer-layout.mobile": {
    "props": {
      "blockClass": "footer-container-mobile"
    },
    "children": [
      "flex-layout.row#footer-row-mobile"
    ]
  },
  /* ---------------------- FLEX LAYOUT ---------------------------- */
  "flex-layout.row#footer-row-mobile": {
    "children": [
      "flex-layout.col#links-mobile",
      "flex-layout.col#follow-us-mobile"
    ],
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "footerRowMobile"
    }
  },
  "flex-layout.col#links-mobile": {
    "props": {
      "blockClass": "links-footer"
    },
    "children": [
      "flex-layout.row#links-row-mobile"
    ]
  },
  "flex-layout.col#follow-us-mobile": {
    "props": {
      "blockClass": "follow-us-footer"
    },
    "children": [
      "flex-layout.row#follow-us-row-mobile"
    ]
  },
  /* ---------------------- LINKS ROW ---------------------------- */
  "flex-layout.row#links-row-mobile": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "links"
    },
    "children": [
      "flex-layout.col#links-left",
      "flex-layout.col#links-right"
    ]
  },
  "flex-layout.col#links-left": {
    "props": {
      "blockClass": "links-left"
    },
    "children": [
      /* "link#store-locator-mobile-label", */
      "link#legal-issues",
      "link#support-mobile",
      "toggle-display#preferences-cookies"
    ]
  },
  "flex-layout.col#links-right": {
    "props": {
      "blockClass": "links-right"
    },
    "children": [
      /* "link#faq-mobile", */
      "link#doittogether",
      "link#safety-mobile",
      "link#manuals"
    ]
  },
  /* ---------------------- LINKS LEFT ---------------------------- */
  "link#store-locator-mobile-label": {
    "props": {
      "href": "/store-locator"
    },
    "children": [
      "rich-text#sl-text"
    ]
  },
  "rich-text#sl-text": {
    "props": {
      "blockClass": "links-text",
      "text": "Store Locator"
    }
  },
  "link#support-mobile": {
