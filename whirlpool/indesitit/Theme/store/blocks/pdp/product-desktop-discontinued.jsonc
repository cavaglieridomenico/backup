    "children": ["product-specification-values#discontinued"]
  },
  "product-specification-text#specification-discontinued": {
    "props": {
      "blockClass": "spec-item-style",
      "message": "{specificationName}"
    }
  },
  "product-specification-values#discontinued": {
    "children": ["product-specification-text#value-discontinued"]
  },
  "product-specification-text#value-discontinued": {
    "props": {
      "message": "{specificationValue}",
      "blockClass": "number-spec"
    }
  },
  "flex-layout.row#accordion-specification-discontinued": {
    "props":{
      "blockClass": "accordion-specification-discontinued"
