    "props": {
      "blockClass": "measure-perks-row-style"
    },
    "children": [
      "flex-layout.col#features-pdp-discontinued",
      "flex-layout.col#measure-discontinued"
    ]
  },
  "customizedMT-pdp#discontinued": {
    "props": {
      "filter": {
        "specificationGroups": ["RatingGroupAttrLogo", "Dimensions"],
        "type": "show"
      }
    }
  },
  "flex-layout.col#features-pdp-discontinued": {
    "props": {
      "width": 50,
      "blockClass": "features-col-style"
    },
    "children": ["flex-layout.row#features-container"]
  },
  "flex-layout.col#measure-discontinued": {
    "props": {
      "width": 50,
      "blockClass": "measure-col-style"
    },
    "children": [
      "rich-text#measures-text-discontinued",
      "product-specification-group#minimal-discontinued"
    ]
  },
  "rich-text#measures-text-discontinued": {
    "props": {
      "text": "Dimensioni",
      "textPosition": "Left",
      "blockClass": "measure-text-style"
    }
  },
  "product-specification-group#minimal-discontinued": {
    "children": ["flex-layout.row#specifications-discontinued"],
    "props": {
      "filter": {
        "specificationGroups": ["Dimensioni"], //, "CategoryDataCluster"
        "type": "show"
      },
      "blockClass": ""
    }
  },
  "flex-layout.row#specifications-discontinued": {
    "props": {
      "blockClass": "product-measure-spec-style"
