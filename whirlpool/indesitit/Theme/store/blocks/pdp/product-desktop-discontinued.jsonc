      "rich-text#out-of-catalog",
      // "button-primary#buy-online"
      "button-primary#buy-online-discontinued"
    ]
  },
  "condition-layout.product#etichetta-image-pre-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "EnergiaLogo"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "label-energie-nouvelle"
          }
        }
      ],
      "matchType": "any",
      "Then": "flex-layout.col#energy-class-new-pdp-discontinued",
      "Else": "condition-layout.product#etichetta-image-pre-old-discontinued"
    }
  },
  "rich-text#out-of-catalog": {
    "props": {
      "blockClass": "out-of-catalog-style",
      "text": "Non disponibile"
    }
  },
  "button-primary#buy-online-discontinued": {
    "props": {
      "text": "Compra simile",
      "isButton": false,
      "width": "231px",
      "blockClass": "discontinued",
      "isDiscontinued": true
    }
  },
  "flex-layout.col#energy-class-new-pdp-discontinued": {
    "children": ["link.product#energyClassNewLinkTab-discontinued"],
    "props": {
      "width": "37%"
    }
  },
  "condition-layout.product#etichetta-image-pre-old-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "energy-label"
          }
        }
      ],
      "Then": "flex-layout.col#energy-class-old-pdp-discontinued"
    }
  },
  "link.product#energyClassNewLinkTab-discontinued": {
    "props": {
      "href": "{specificationGroups.Document.specifications.new-energy-label}",
      "blockClass": "tabPDP",
      "target": "_blank",
      "escapeLinkRegex": "[%+]"
    },
    "children": [
      "image-from-specification#EtichettaEnergetica-discontinued",
      "rich-text#product-energy-class-btn-discontinued"
    ]
  },
  "flex-layout.col#energy-class-old-pdp-discontinued": {
    "children": ["link.product#energyClassOldLinkTab-discontinued"],
    "props": {
      "width": "37%"
    }
  },
  "image-from-specification#EtichettaEnergetica-discontinued": {
    "props": {
      "blockClass": "energy-class-icon-style",
      "specificationName": "EnergyLogo_image",
      "imageLink": "Specification.EnergyLogo_image",
      "labelWithHref": true,
      "target": "_blank"
    }
  },
  "rich-text#product-energy-class-btn-discontinued": {
    "props": {
      "blockClass": "energy-class-btn",
      "text": "Classe energetica"
    }
  },
  "link.product#energyClassOldLinkTab-discontinued": {
    "props": {
      "href": "{specificationGroups.Document.specifications.energy-label}",
      "blockClass": "tabPDP",
      "target": "_blank",
      "escapeLinkRegex": "[%+]"
    },
    "children": [
      "image-from-specification#EtichettaEnergetica-discontinued",
      "rich-text#product-energy-class-btn-discontinued"
    ]
  },
  "condition-layout.product#customizedMT-discontinued": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "customizedMT"
          }
        }
      ],
      "Then": "flex-layout.row#customizedMT-discontinued",
      "Else": "flex-layout.row#measures-perks-card-discontinued"
    }
  },
