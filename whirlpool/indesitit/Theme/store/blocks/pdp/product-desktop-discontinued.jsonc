    "children": ["flex-layout.row#spec-item-discontinued"]
  },
  "flex-layout.row#spec-item-discontinued": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "spec-item-style"
    },
    "children": [
      "flex-layout.col#spec-discontinued",
      "flex-layout.col#prod-spec-values-discontinued"
    ]
  },
  "flex-layout.col#spec-discontinued": {
    "props": {
      "blockClass": "prod-spec"
    },
    "children": ["product-specification-text#specification-discontinued"]
  },
  "flex-layout.col#prod-spec-values-discontinued": {
    "props": {
      "blockClass": "prod-spec-values"
