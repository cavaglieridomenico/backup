  "condition-layout.product#warranty": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "extendedWarranty",
            "value": "true"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#banner-warranty-10"
    }
  },
  "condition-layout.product#warranty-over": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "extendedWarranty",
            "value": "25"
          }
        }
      ],
      "Then": "flex-layout.row#banner-warranty-25",
      "Else": "condition-layout.product#warranty-over25"
    }
