    "props": {
      "visible": true
    },
    "children": ["rich-text#BannTitleTopBlackFriday2"]
  },
  "experimental__visibility-layout#BannDescTopBlackFriday2": {
    "props": {
      "visible": true
    },
    "children": ["rich-text#BannDescTopBlackFriday2"]
  },
  "experimental__visibility-layout#ButtonBannerTopBlackFriday2": {
    "props": {
      "visible": true
    },
    "children": ["link#ButtonBannTopBlackFriday2"]
  },
  "rich-text#BannTitleTopBlackFriday2": {
    "props": {
      "text": "# Fino al 20% di sconto sull’acquisto di un’ampia selezione di elettrodomestici. ",
      "blockClass": "titleHBannerPostilla"
    }
  },

  "rich-text#BannDescTopBlackFriday2": {
    "props": {
      "text": "\n * **10%** con una spesa di almeno **300€ e fino a 499€** \n * **15%** con una spesa di almeno **500€ e fino a 899€** \n * **20%** con una spesa di oltre **900€**",
      "blockClass": "descBannerPostilla"
    }
  },
  "link#ButtonBannTopBlackFriday2": {
    "props": {
      "label": "SCOPRI I PRODOTTI",
      "href": "https://www.whirlpool.it/promozioni/whirlpool-black-friday/sconto-percentuale",
      "displayMode": "button",
      "blockClass": "postillaButton"
    }
  },
