  "image#bannerTopImageInvertedBlackFriday2": {
    "props": {
      "src": "assets/BlackFriday_soglia2.png",
      "blockClass": ["cover", "imagePostillaBanner"]
    }
  },
  "flex-layout.col#leftPartTopInvertedBlackFriday2": {
    "props": {
      "width": "50%",
      "preventVerticalStretch": true,
      "blockClass": ["columnPostilla", "columnPostillaBottom"]
