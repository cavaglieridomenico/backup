  "flex-layout.row#titleScontiExtraBlackFriday2": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "marginButton1Rem"
    },
    "children": [
      "rich-text#titleBlackFridayBlackFriday2",
      "rich-text#titleScontiExtraBlackFriday2"
    ]
