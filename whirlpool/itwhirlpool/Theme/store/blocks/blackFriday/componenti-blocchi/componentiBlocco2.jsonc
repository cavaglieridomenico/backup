  "rich-text#dataScontiExtraBlackFriday2": {
    "props": {
      "text": "Dal 26 al 28 novembre",
      "blockClass": "durata"
    }
  },
  "rich-text#titleBlackFridayBlackFriday2": {
    "props": {
      "text": "",
      "blockClass": ["title-blackFriday", "marginRightDot3", "noMarginTop"]
    }
  },
