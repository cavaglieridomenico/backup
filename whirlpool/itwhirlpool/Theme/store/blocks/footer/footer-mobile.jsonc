{
  "flex-layout.row#accordionFooter": {
    "props": {
      "blockClass": ["disclosureFooter"]
    },
    "children": [
      "disclosure-layout#servizio-clienti",
      "disclosure-layout#collezioni",
      "disclosure-layout#le-nostre-policy",
      "disclosure-layout#elettrodomestici",
      "disclosure-layout#accessori",
      "disclosure-layout#la-nostra-azienda"
    ]
  },
  "disclosure-state-indicator#footer": {
    "props": {
      "Show": "image#PlusIcon",
      "Hide": "image#MinusIcon"
    }
  },
  "image#PlusIcon": {
    "props": {
      "src": "assets/icons/angle-down.png",
      "blockClass": "PlusIcon",
      "width": "24px",
      "height": "24px"
    }
  },
  "image#MinusIcon": {
    "props": {
      "src": "assets/icons/angle-up.png",
      "blockClass": "MinusIcon",
      "width": "24px",
      "height": "24px"
    }
  },
  "disclosure-layout#collezioni": {
    "props": {
      "blockClass": "disclosureFooter"
    },
    "children": [
      "disclosure-trigger#collezioni",
      "disclosure-content#collezioni"
    ]
  },
