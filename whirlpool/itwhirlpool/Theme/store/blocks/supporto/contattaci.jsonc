{
  "store.custom#contattaci": {
    "blocks": [
      "modal-trigger#oow-popup",
      "sticky-layout#supportoCardsBottom",
      "supporto-sticky-menu",
      "toggle-layout#chatWidget",
      "__fold__.experimentalLazyAssets",
      "flex-layout.row#bread-theme",
      "experimental__visibility-layout#bannerTopContattaci",
      "slider-layout#supportoCardsBottom",
      "flex-layout.row#space",
      "contact-us-form#contact-us-aggiornata",
      "slider-layout#supportoCardsBottom",
      "newsletter-popup#footer"
      //"analytics#contact"
    ]
  },
  "breadcrumb#contattaci": {
    "props": {
      "categories": [
        "supporto",
        "contact-us-form"
      ],
      "showOnMobile": false
    }
  },
  "flex-layout.row#space": {
    "props": {
      "htmlId":"space-contact-us-form",
      "blockClass": "rowSpace"
    }
  },
  "contact-us-form#contact-us-aggiornata": {
    "props": {
      "apiAccountSigle": "CT",
      "telephonePlaceholder": "+39",
      "emailPlaceholder": "latuaemail@prova.com",
      "datePickerPlaceholder": "GG/MM/AAAA",
      "datePickerLocale": "it",
      "reasons": [
        {
          "label": "Informazioni sui prodotti",
          "SupportEmail": "gestioneconsumatore@whirlpool.com"
        },
        {
          "label": "Informazioni acquisti online sul sito www.whirlpool.it",
          "SupportEmail": "supporto@whirlpool.com"
        },
        {
          "label": "Richiesta assistenza tecnica su prodotto 0-24 mesi",
          "SupportEmail": "assistenza@whirlpool.com"
        },
        {
          "label": "Richiesta assistenza tecnica su prodotto con più di 24 mesi",
          "SupportEmail": "itservice199@whirlpool.com"
        },
        {
          "label": "Richiesta assistenza tecnica su prodotto coperto da estensione di assistenza",
          "SupportEmail": "assistenza@whirlpool.com"
        },
        {
          "label": "Informazioni/Segnalazioni intervento in corso",
          "SupportEmail": "gestioneconsumatore@whirlpool.com"
        },
        {
          "label": "Spostare, modificare o cancellare un appuntamento",
          "SupportEmail": "supporto@whirlpool.com"
        },
        {
          "label": "Aggiornare /cancellare dati personali (gestione privacy) / GDPR cancellazione account",
          "SupportEmail": "supporto@whirlpool.com"
        }
      ]
    }
  },
  "info-card#heroBannerContact-us": {
    "props": {
      "id": "hero-banner-contact-us",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/hero-banner-contact-us.jpg",
      "headline": "Contattaci",
      "subhead": "Il nostro team di assistenza clienti è disponibile tutti i giorni all'anno (*) per fornire consulenza e soluzioni per aiutarti a gestire, mantenere e riparare i tuoi elettrodomestici e farli funzionare come nuovi. \n\n\n\n\n\n\n\n\n (*) ad eccezione dei giorni festivi legalmente riconosciuti.",
      "callToActionMode": "none",
      "blockClass": [
        "heroBanner",
        "heroBannerSearch"
      ],
      "textAlignment": "left"
    }
  },
  "image#hero-banner-contact-us": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_pianicottura.jpg"
    }
  },
  "image#matricolaPianicottura": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_pianicottura.jpg"
    }
  },
  "image#matricolaAsciugatrici": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_asciugatrici.png"
    }
  },
  "image#matricolaCucine": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_cucine.png"
    }
  },
  "image#matricolaForni": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_forni.png"
    }
  },
  "image#matricolaFrigoriferi": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_frigoriferi.png"
    }
  },
  "image#matricolaLavasciuga": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_lavasciuga.png"
    }
  },
  "image#matricolaLavastoviglie": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_lavastoviglie.png"
    }
  },
  "image#matricolaLavatrici": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_lavatrici.png"
    }
  },
  "image#matricolaCongelatori": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_congelatori.png"
    }
  },
  "experimental__visibility-layout#bannerTopContattaci": {
    "props": {
      "visible": true
    },
    "children": [
      "flex-layout.row#bannerTopContattaci"
    ]
  },
  "flex-layout.row#bannerTopContattaci": {
    "props": {
      "preventHorizontalStretch": false,
      "blockClass": [
        "bannerPostilla",
        "minHeigth26rem",
        "marginBottom2rem"
      ]
    },
    "children": [
      "flex-layout.col#leftPartTopContattaci",
      "flex-layout.col#rightPartTopContattaci"
    ]
  },
  "flex-layout.col#leftPartTopContattaci": {
    "props": {
      "width": "40%",
      "preventVerticalStretch": true,
      "blockClass": [
        "columnPostilla",
        "columnPostillaBottom"
      ]
    },
    "children": [
      "experimental__visibility-layout#logoBannerTopContattaci",
      "experimental__visibility-layout#TitleBannerTopContattaci",
      "experimental__visibility-layout#BannDescTopContattaci",
      "experimental__visibility-layout#ButtonBannerTopContattaci",
      "experimental__visibility-layout#bannerTopPostillaContattaci"
    ]
  },
  "experimental__visibility-layout#logoBannerTopContattaci": {
    "props": {
      "visible": false
    },
    "children": [
      "image#LogoBannerTopContattaci"
    ]
  },
  "experimental__visibility-layout#TitleBannerTopContattaci": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#BannTitleTopContattaci"
    ]
  },
  "experimental__visibility-layout#BannDescTopContattaci": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#BannDescTopContattaci"
    ]
  },
  "experimental__visibility-layout#ButtonBannerTopContattaci": {
    "props": {
      "visible": false
    },
    "children": [
      "link#ButtonBannTopContattaci"
    ]
  },
  "experimental__visibility-layout#bannerTopPostillaContattaci": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#bannerTopPostillaContattaci"
    ]
  },
  "image#LogoBannerTopContattaci": {
    "props": {
      "src": "assets/postillaBanner/logoGoogle.jpg",
      "width": "5rem",
      "blockClass": "marginBottom"
    }
  },
  "flex-layout.col#rightPartTopContattaci": {
    "props": {
      "width": "60%",
      "blockClass": "columnPostillaImage"
    },
    "children": [
      "image#bannerTopImageContattaci"
    ]
  },
  "rich-text#BannTitleTopContattaci": {
    "props": {
      "text": "# Contattaci",
      "blockClass": "titleHBannerPostilla"
    }
  },
  "rich-text#BannDescTopContattaci": {
    "props": {
      "text": "Il nostro team di assistenza clienti è disponibile tutti i giorni all'anno (*) per fornire consulenza e soluzioni per aiutarti a gestire, mantenere e riparare i tuoi elettrodomestici e farli funzionare come nuovi.",
      "blockClass": [
        "descBannerPostilla",
        "descContattaci"
      ]
    }
  },
  "link#ButtonBannTopContattaci": {
    "props": {
      "label": "TESTO BOTTONE",
      "href": "#",
      "displayMode": "button",
      "blockClass": "postillaButton"
    }
  },
  "rich-text#bannerTopPostillaContattaci": {
    "props": {
      "text": "(*) ad eccezione dei giorni festivi legalmente riconosciuti.",
      "blockClass": "postillaBannerPostilla"
    }
  },
  "image#bannerTopImageContattaci": {
    "props": {
      "src": "assets/postillaBanner/contattaci.jpg",
      "blockClass": [
        "imagePostillaBanner",
        "minHeightCover"
      ]
    }
  }
}
