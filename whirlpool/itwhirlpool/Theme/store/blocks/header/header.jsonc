    "children": ["popup-custom#home"]
  },
  "popup-custom#home": {
    "props": {
      "blockClass": "popupHome"
    },
    "children": ["image#popup-home", "rich-text#popup-home", "analytics-wrapper#popup-home"]
  },
  "image#popup-home": {
    "title": "Icona dello Stripe - Pop Up Banner",
    "props": {
      "blockClass": "popupHome",
      "alt": "icon of stripe",
      "src": "assets/account-icon.svg",
      "width": 24,
      "height": 24
    }
  },
  "rich-text#popup-home": {
    "title": "Testo dello Stripe - Pop Up Banner",
    "props": {
      "blockClass": "popupHome",
      "text": "TESTO DI PROVA DELLO STRIPE - POP UP CUSTOM IN HOMEPAGE",
      "textAlignment": "LEFT",
      "textPosition": "CENTER"
    }
  },
  "analytics-wrapper#popup-home": {
    "title": "[Analytics] CTA dello Stripe",
    "props": {
      "isSlider": false,
      "isLazy": false,
      "propsAnalytics": {
        "analyticsProperties": "provide",
        "nameEvent": "stripeBanner_clickCTA",
        "name": "data",
        "data": [
          {
            "title": "section",
            "value": "Homepage"
          }
        ]
      },
      "isView": true
    },
    "children": ["link#popup-home"]
  },
  "link#popup-home": {
    "title": "CTA dello Stripe - Pop Up Banner",
    "props": {
      "blockClass": "popupHome",
      "label": "Scopri di più",
      "href": "#",
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "small"
      },
      "isPrevent": false
    }
  },
