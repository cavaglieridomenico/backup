        "margin"
      ]
    }
  },
  "search-bar#headerBlock": {
    "blocks": [
      "autocomplete-result-list.v2"
    ],
    "props": {
      "openAutocompleteOnFocus": true,
      "blockClass": "headerSearch"
    }
  },
  "autocomplete-result-list.v2": {
    "blocks": [
      "product-summary.shelf#headerSearch"
    ],
    "props": {
      "maxSuggestedProducts": 8,
      "maxSuggestedTerms": 3,
      "maxTopSearches": 5,
      "maxHistory": 3,
      "productLayout": "VERTICAL"
      // "hideTitles": true
    }
  },
  "product-summary.shelf#headerSearch": {
    "children": [
      "condition-layout.product#inStockLabel-SearchResult-PreOrderLabel",
      "product-summary-image#headerSearchShelf",
      "vtex.store-components:product-name#headerSearchSummary",
      "condition-layout.product#showPricePDPSelling-SearchResult"
    ],
    "props": {
      "blockClass": "shelfHeaderSearch"
    }
  },
  "condition-layout.product#inStockLabel-SearchResult-PreOrderLabel": {
    "props": {
      "conditions": [
        {
          "subject": "productClusters",
          "arguments": {
            "id": "419"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "DISPONIBILITÀ",
            "value": "Prodotti disponibili"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "true"
          }
        },
        {
          "subject": "isProductAvailable"
        }
      ],
      "matchType": "all",
      "Then": "rich-text#preOrder-SearchResult-SearchResult-preOrder",
      "Else": "condition-layout.product#inStockLabel-SearchResult"
    }
  },
  "rich-text#preOrder-SearchResult-SearchResult-preOrder": {
    "props": {
      "text": "Preordine",
      "blockClass": ["specProps", "SearchResult", "preOrder"]
    }
  },
  "condition-layout.product#inStockLabel-SearchResult": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#inStockLabelSecond-SearchResult",
      "Else": "rich-text#outOfStock-SearchResult"
    }
  },
  "condition-layout.product#inStockLabelSecond-SearchResult": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "DISPONIBILITÀ",
            "value": "Prodotti disponibili"
          }
        },
        {
          "subject": "isProductAvailable"
        }
      ],
      "matchType": "all",
      "Then": "flex-layout.col#stock-SearchResult",
      "Else": "rich-text#outOfStock-SearchResult"
    }
  },
  "rich-text#outOfStock-SearchResult": {
    "props": {
      "text": "Non disponibile",
      "blockClass": ["specProps", "SearchResult"]
    }
  },
  "flex-layout.col#stock-SearchResult": {
    "children": ["print-specification#stock-SearchResult"],
    "props": {
      "blockClass": ["specProps", "inStockProp", "SearchResult"]
    }
  },
  "print-specification#stock-SearchResult": {
    "props": {
      "specificationsToCheck": [
        {
          "name": "sellable"
        }
      ],
      "blockClass": ["SearchResult"],
      "aggregateLabel": "Disponibile"

    }
  },
  "condition-layout.product#showPricePDPSelling-SearchResult": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "flex-layout.row#price-selling-pdp-SearchResult"
    }
  },
  "flex-layout.row#price-selling-pdp-SearchResult": {
    "children": [
      "flex-layout.col#product-selling-price-SearchResult"
    ],
    "props": {
      "blockClass": ["discountedPrice", "SearchResult"],
      "colSizing": "auto",
      "fullWidth": true,
      "preserveLayoutOnMobile": true,
      "horizontalAlign": "left"
    }
  },
  "flex-layout.col#product-selling-price-SearchResult": {
    "children": [
      "product-selling-price#pdp-SearchResult"
    ]
  },
  "product-selling-price#pdp-SearchResult": {
    "props": {
      "blockClass":["summary2", "SearchResult"],
      "markers": [
        "Tax"
      ],
      "message": "{sellingPriceValue}"
    }
  },
  "product-summary-image#headerSearchShelf": {
    "props": {
      "showBadge": false,
      "aspectRatio": "1:1",
      "maxHeight": 100,
      "blockClass": "headerSearchShelfImage"
