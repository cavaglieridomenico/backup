        "title": "[EXPERIMENTAL-VISIBILITY] - Carousel",
        "props": {
            "visible": true
        },
        "children": [
            "list-context.product-list#carouselRemoteHTMLTemplateMobile"
        ]
    },
    "list-context.product-list#carouselRemoteHTMLTemplateMobile": {
        "title": "[LIST-CONTEXT] - Product List",
        "blocks": [
            "product-summary.shelf.product-comparison#searchNew"
        ],
        "children": [
            "slider-layout-whl#carouselCollectionHomeMobile"
