  "info-card#heroBannerContact-us": {
    "props": {
      "id": "hero-banner-contact-us",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/hero-banner-contact-us.jpg",
      "headline": "Contattaci",
      "subhead": "Il nostro team di assistenza clienti è disponibile tutti i giorni all'anno (*) per fornire consulenza e soluzioni per aiutarti a gestire, mantenere e riparare i tuoi elettrodomestici e farli funzionare come nuovi. \n\n\n\n\n\n\n\n\n (*) ad eccezione dei giorni festivi legalmente riconosciuti.",
      "callToActionMode": "none",
      "blockClass": ["heroBanner", "heroBannerSearch"],
      "textAlignment": "left"
    }
  },
  "image#hero-banner-contact-us": {
    "props": {
      "src": "assets/contattaci-matricola-prodotti/matricola_prodotto_pianicottura.jpg"
    }
  },
