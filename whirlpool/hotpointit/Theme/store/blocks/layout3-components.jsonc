{
  "flex-layout.row#pageDescriptionBlackFriday": {
    "children": [
      "rich-text#pageDescriptionBlackFriday"
    ]
  },
  "rich-text#pageDescriptionBlackFriday": {
    "title": "Rich Text - Page description",
    "props": {
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "pageDescriptionBlackFriday",
      "text": "# Promo Hotpoint IT \n Offerte lampo valide solo online su un'ampia \n selezione di prodotti. \n Acquistando online, potrai ricevere comodamente a casa il tuo elettrodomestico al miglior prezzo."
    }
  },
  "flex-layout.row#separator": {
    "props": {
      "blockClass": "separator"
    }
  },
  "toggle-layout#timer-desktop-1": {
    "title": "Toggle Layout - Timer Desktop 1",
    "props": {
      "renderChildren": false
    },
    "children": [
      "flex-layout.row#separator",
      "timer-component#timer-1"
    ]
  },
  "timer-component#timer-1": {
    "props": {
      "showDateFormat": true,
      "targetDate": "2021-11-30",
      "ore": "00",
      "minuti": "00",
      "secondi": "00",
      "Head": "flex-layout.row#ContainerScontiExtraBlackFriday3",
      "Else": "flex-layout.row#ElseBlackFridayBlackFriday3"
    },
    "children": [
      "toggle-layout#carouselPromo",
      "flex-layout.row#scopriDiPiùBlackFriday3"
    ],
    "title": "Timer - 1"
  },
  /*Head (PART SHOWN WHEN PROMO IS ACTIVE)*/
  "flex-layout.row#ContainerScontiExtraBlackFriday3": {
    "title": "Part Shown With Timer Active",
    "props": {},
    "children": [
      "flex-layout.col#head-sconti-extraBlackFriday3"
    ]
  },
  "flex-layout.col#head-sconti-extraBlackFriday3": {
    "children": [
      "flex-layout.row#dataScontiExtraBlackFriday3",
      "flex-layout.row#titleScontiExtraBlackFriday3",
      "flex-layout.row#bannerWithPostillaPromo"
