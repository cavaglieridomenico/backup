    "children": [
      "rich-text#dataScontiExtraBlackFriday3"
    ]
  },
  "rich-text#dataScontiExtraBlackFriday3": {
    "title": "Rich Text - Promo Date",
    "props": {
      "text": "Solo oggi - 29 novembre",
      "blockClass": "durata"
    }
  },
  "flex-layout.row#titleScontiExtraBlackFriday3": {
    "props": {
      "preventHorizontalStretch": true,
      "blockClass": "marginButton1Rem"
    },
    "children": [
      "rich-text#titleBlackFridayBlackFriday3",
      "rich-text#titleScontiExtraBlackFriday3"
    ]
  },
  "rich-text#titleBlackFridayBlackFriday3": {
    "title": "Rich Text - Title 1",
    "props": {
      "text": "",
      "blockClass": [
        "title-blackFriday",
        "marginRightDot3",
        "noMarginTop"
      ]
    }
  },
  "rich-text#titleScontiExtraBlackFriday3": {
    "title": "Rich Text - Promo Title",
    "props": {
      "text": "### RINNOVA LA TUA CASA CON HOTPOINT",
      "blockClass": [
        "title-section-blackFriday",
        "noMarginTop"
      ]
    }
  },
  "flex-layout.row#bannerWithPostillaPromo": {
    "title": "Row - Banner with Image",
    "children": [
      "toggle-layout#bannerWithPostillaPromo"
    ]
  },
  "toggle-layout#bannerWithPostillaPromo": {
    "title": "Toggle Layout - Banner With Image",
    "children": [
