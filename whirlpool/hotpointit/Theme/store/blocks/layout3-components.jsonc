    ]
  },
  "flex-layout.row#bannerWithImagePromo": {
    "props": {
      "preventHorizontalStretch": false,
      "blockClass": [
        "columnPostillaBannerPromo"
      ]
    },
    "children": [
      "flex-layout.col#leftPartPostillaPromo",
      "flex-layout.col#rightPartPostillaPromo"
    ]
  },
  "flex-layout.col#leftPartPostillaPromo": {
    "title": "Left Part Banner",
    "props": {
      "width": "50%",
      "blockClass": "columnPostillaImage"
    },
    "children": [
      "image#bannerPostilla"
    ]
  },
  "flex-layout.col#rightPartPostillaPromo": {
    "title": "Right Part Banner",
    "props": {
      "width": "50%",
      "preventVerticalStretch": true,
      "blockClass": [
        "columnPostilla",
        "columnPostillaBottom"
      ]
    },
    "children": [
      "rich-text#BannTitle",
      "rich-text#BannDesc",
      "toggle-layout#ButtonBannerWithPostilla"
    ]
  },
