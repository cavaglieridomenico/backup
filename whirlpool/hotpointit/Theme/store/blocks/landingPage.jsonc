  "link#ButtonBannRecensioniBlu": {
    "props": {
      "label": "Registra il prodotto",
      "href": "#",
      "displayMode": "button",
      "blockClass": [
        "postillaButtonLanding",
        "postillaButtonRecensioniBlu"
      ]
