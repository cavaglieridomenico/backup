      "toggle-layout#title3",
      "list-context.product-list#suggestions-w-cappe",
      "flex-layout.row#newsletter-row"
    ]
  },
  "toggle-layout#title1": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#TitlePreCarousel"
    ]
  },
  "toggle-layout#title2": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#TitlePreCarousel"
    ]
  },
  "toggle-layout#title3": {
    "props": {
      "visible": true
    },
    "children": [
      "rich-text#TitlePreCarousel"
    ]
  },
  "rich-text#TitlePreCarousel": {
    "props": {
      "text": "### Titolo del carosello",
      "blockClass": "carouselTitle",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "toggle-layout#bannerLanding": {
    "props": {
      "visible": true
    },
    "children": [
      "flex-layout.row#bannerLanding"
    ]
  },
  "flex-layout.row#bannerLanding": {
    "props": {
      "preventHorizontalStretch": false,
      "blockClass": [
        "columnPostillaBannerLanding"
      ]
    },
    "children": [
      "flex-layout.col#textBannerLanding"
    ]
  },
  "flex-layout.col#textBannerLanding": {
    "props": {
      "width": "100%",
      "preventVerticalStretch": true,
      "blockClass": [
        "columnPostilla",
        "columnPostillaLanding"
      ]
    },
    "children": [
      "toggle-layout#LogoBannerTopCarousel2",
      "toggle-layout#BannTitle",
      "toggle-layout#BannDesc",
      "toggle-layout#ButtonBannerWithPostillaLanding",
      "toggle-layout#BannPostilla"
    ]
  },
  "toggle-layout#LogoBannerTopCarousel2": {
    "props": {
      "visible": true
    },
    "children": [
      "image#LogoBannerTopCarousel"
    ]
  },
  "rich-text#BannDescLanding": {
    "props": {
      "text": "Descrizione",
      "blockClass": "descBannerPostillaLanding"
    }
  },
  "toggle-layout#ButtonBannerWithPostillaLanding": {
    "props": {
      "visible": true
    },
    "children": [
      "toggle-layout#ButtonBannerWithPostillaOrange",
      "toggle-layout#ButtonBannerWithPostillaWhite"
    ]
  },
  "toggle-layout#ButtonBannerWithPostillaOrange": {
    "props": {
      "visible": true
    },
    "children": [
      "link#ButtonBannOrange"
    ]
  },
  "link#ButtonBannOrange": {
    "props": {
      "label": "TESTO BOTTONE",
      "href": "#",
      "displayMode": "button",
      "blockClass": "postillaButtonLanding"
    }
  },
  "toggle-layout#ButtonBannerWithPostillaWhite": {
    "props": {
      "visible": true
    },
    "children": [
      "link#ButtonBannWhite"
    ]
  },
  "link#ButtonBannWhite": {
    "props": {
      "label": "TESTO BOTTONE",
      "href": "#",
      "displayMode": "button",
      "blockClass": "postillaButtonLandingWhite"
    }
  },
