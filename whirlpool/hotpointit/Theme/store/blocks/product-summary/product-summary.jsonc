  "condition-layout.product#showPriceOrNot": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "flex-layout.row#list-price-plp-mobile"
    }
  },
  "condition-layout.product#showPriceOrNotProduct-price-savings#saving": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "product-price-savings#saving"
      // "product-price-savings#saving-pdpMobile"
    }
  },
  "condition-layout.product#showPriceUnavailableProd": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "condition-layout.product#showPriceAndIsNotAvilable"
    }
  },
  "condition-layout.product#showPriceAndIsNotAvilable": {
    "props": {
      "conditions": [
        {
          "subject": "isProductAvailable"
        }
      ],
      "Then": "flex-layout.row#selling-price-savings",
      "Else": "flex-layout.row#priceUnavailableProd"
    }
  },
  "product-price-savings#saving": {
    "props": {
      "markers": [
        "highlight"
      ],
      "blockClass": "saving",
