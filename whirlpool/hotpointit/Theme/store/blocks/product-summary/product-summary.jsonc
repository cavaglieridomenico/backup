  "flex-layout.row#productType": {
    "children": [
      "flex-layout.col#wcollection",
      "flex-layout.col#badge"
    ],
    "props": {
      "preserveLayoutOnMobile": true,
      "preventHorizontalStretch": false,
      "blockClass": "badges",
      "colSizing": "equal"
