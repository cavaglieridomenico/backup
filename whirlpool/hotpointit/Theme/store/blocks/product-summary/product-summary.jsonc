  "product-installments#summary": {
    "props": {
      "blockClass": "summary"
    }
  },
  "condition-layout.product#showPriceOrNotProduct-selling-price-PLP": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPrice",
            "value": "true"
          }
        }
      ],
      "Then": "product-selling-price#summary"
    }
  },
  "product-selling-price#summary": {
    "props": {
      "alwaysShow": true,
      "blockClass": [
        "summary",
        "summaryPL"
      ],
      "markers": [
        "Tax"
      ],
      "message": "{sellingPriceValue} IVA inclusa "
    }
  },
  "product-price-savings#summary": {
    "props": {
      "markers": [
        "discount"
      ],
      "message": "{savingsPercentage}",
      "blockClass": [
        "summary",
        "padding",
        "backWhitheBorderFdc100ColorFdc100"
      ]
    }
  },
  "flex-layout.row#dimensioni": {
    "props": {
      "blockClass": "dimensioni"
    },
    "children": [
      "print-specification#dimensioni"
    ]
