  "experimental__visibility-layout#masterchefBanner": {
    "title": "[Toggle] - Masterchef First Banner",
    "props": {
      "visible": true
    },
    "children": ["image#heroBannerMasterchef"]
  },
  "image#heroBannerMasterchef": {
    "title": "[Image] - Masterchef First Banner",
    "props": {
      "src": "https://hotpointit.vtexassets.com/assets/vtex.file-manager-graphql/images/f8bb9dd2-108a-4277-b3db-10ddafe9f05a___f2a4cc6d4977013a654770925f083b4e.jpg",
      "blockClass": "masterchefFirstBanner",
      "alt": "Masterchef banner"
    }
  },
