  "toggle-layout#halfBannerMasterchef": {
    "props": {
      "visible": true
    },
    "children": [
      "info-card#halfBannerMasterchef",
      "flex-layout.row#halfBannerMasterchef"
    ]
  },
  "flex-layout.row#heroBannerMasterchef": {
    "props": {
      "blockClass": "heroBannerMasterchef"
    },
    "children": [
      "image#logoHotpoint",
      "rich-text#logoHotpoint",
      "rich-text#titleHotpoint",
      "image#logoMasterchef"
    ]
  },
  "flex-layout.row#halfBannerMasterchef": {
    "props": {
      "blockClass": [
        "halfBannerMasterchef",
        "halfBannerMasterchef"
      ]
    },
    "children": [
      "rich-text#halfBannerTitleHotpoint",
      "rich-text#halfbannerSubTitle"
    ]
  },
  "image#logoHotpoint": {
    "props": {
      "src": "assets/hotpoint-logo.svg",
      "width": "6.25rem",
      "blockClass": "logoHotpointMasterchef"
    }
  },
  "image#logoMasterchef": {
    "props": {
      "src": "assets/hotpoint-logo.svg",
      "width": "6.25rem",
      "blockClass": "logoMasterchef"
    }
  },
  "rich-text#logoHotpoint": {
    "props": {
      "text": "# Gli elettrodomestici scelti da",
      "blockClass": "titleMasterchef"
    }
  },
  "rich-text#halfbannerSubTitle": {
    "props": {
      "text": "# Prestazioni elevate per risultati professionali",
      "blockClass": [
        "titleMasterchef",
        "halfBannerMasterchef",
        "halfBannerMasterchefSubTitle"
      ]
    }
  },
  "rich-text#titleHotpoint": {
    "props": {
      "text": "# Masterchef Italia",
      "blockClass": [
        "titleMasterchef",
        "titleMasterchefTitle"
      ]
    }
  },
  "rich-text#halfBannerTitleHotpoint": {
    "props": {
      "text": "# Elettrodomestici Hotpoint",
      "blockClass": [
        "titleMasterchef",
        "titleMasterchefTitle",
        "halfBannerMasterchef"
      ]
    }
  },
  "info-card#heroBannerMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/supremecareHEROlarge.jpg",
      "callToActionMode": "CallToActionEnum",
      "blockClass": [
        "heroBannerChef",
        "heroBannerSearchChef"
      ],
      "textAlignment": "left"
    }
  },
  "info-card#halfBannerMasterchef": {
    "props": {
      "id": "hero-banner-main-search",
      "isFullModeStyle": false,
      "textPosition": "left",
      "imageUrl": "assets/supremecareHEROlarge.jpg",
      "callToActionMode": "CallToActionEnum",
      "blockClass": [
        "halfBannerChef",
        "halfBannerSearchChef"
      ],
      "textAlignment": "left"
    }
  },
  "flex-layout.row#countdownStripe": {
    "props": {
      "fullWidth": true,
      "blockClass": [
        "countdownStripe"
      ]
    },
    "children": [
      "rich-text#countdownStripe",
      "rich-text#countdownStripeDown"
    ]
