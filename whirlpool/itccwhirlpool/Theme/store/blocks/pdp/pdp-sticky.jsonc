  "flex-layout.col#pdpStickyColPrice": {
    "props": {
      "blockClass": "stickyRight"
    },
    "children": ["flex-layout.row#showPriceSticky"]
  },
  "flex-layout.col#pdpStickyCol2": {
    "props": {
      "blockClass": "stickyClasseEnergitica"
    },
    "children": [
