{
  "condition-layout.product#stickyAddToCart": {
    "title": " [CONDITION] isProductAvailable? showAddToCArtSticky",
    "props": {
      "conditions": [
        {
          "subject": "isProductAvailable"
        }
      ],
      "Then": "flex-layout.row#pdpSticky"
    }
  },
  "sticky-layout#addToCart-button": {
    "children": [
      "flex-layout.row#pdpSticky",
      // ready to use when BI asks
      "experimental__visibility-layout#promoStickyBannerUnderAddToCArt"
      // "flex-layout.row#promoStickyBannerUnderAddToCArt"
    ],
    "props": {
      "blockClass": ["stickyButton", "mw9", "ph3"]
    }
  },
  "experimental__visibility-layout#promoStickyBannerUnderAddToCArt": {
    "props": {
      "visible": false
    },
    "children": ["flex-layout.row#promoStickyBannerUnderAddToCArt"]
  },
  "flex-layout.row#promoStickyBannerUnderAddToCArt": {
    "title": "[ROW] - Sticky Promo Banner",
    "children": [
      "vtex.product-highlights@2.x:product-highlights#promoStickyBanner"
    ],
    "props": {
      "blockClass": ["promoStickyBanner", "promoStickyBannerUnderAddToCArt"]
    }
  },
  "flex-layout.row#pdpSticky": {
    "props": {
      "blockClass": "stickyContainer",
      "preserveLayoutOnMobile": true
    },
    "children": [
      "flex-layout.col#pdpStickyColImage",
      "flex-layout.col#pdpStickyColPrice"
    ]
  },

  "flex-layout.col#pdpStickyColImage": {
    "props": {
      "blockClass": "stickyLeft"
    },
    "children": [
      "flex-layout.row#pdpStickyImageProdotto"
      // "condition-layout.product#isAccessorioTabSticky"
    ]
  },
  "vtex.menu@2.x:menu#pdpTabSticky": {
