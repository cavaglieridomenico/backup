  "flex-layout.col#EtichettaEnergeticaNew": {
    "props": {
      "blockClass": "EtichettaEnergeticaNewSticky"
    },
    "children": ["image-from-specification#EtichettaEnergeticaNew"]
  },
  "flex-layout.col#pdpStickyCol3": {
    "props": {
      "blockClass": "nameClasseEnergeticaSticky"
    },
