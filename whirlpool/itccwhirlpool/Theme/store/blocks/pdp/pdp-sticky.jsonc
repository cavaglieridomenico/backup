  "flex-layout.row#showPriceSticky": {
    "props": {
      "blockClass": "showPriceSticky"
    },
    "children": [
      "product-price-custom#sticky",
      "condition-layout.binding#checkAccessoriesAndAvailabilityOnBinding"
    ]
  },
  "product-price-custom": {
    "props": {
      "showStar": false
    }
  },
  "product-price-custom#sticky": {
    "props": {
      "showStar": false
    },
    "children": ["product-price-savings#saving-pdp-sticky"]
  },
  "condition-layout.product#showPriceSticky": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "showPriceEPP",
            "value": "true"
          }
        }
      ],
      "Then": "flex-layout.col#price"
    }
  },
  "flex-layout.col#price": {
    "props": {
      "blockClass": "priceAndDiscountSticky"
    },
    "children": [
      "flex-layout.row#list-price-saving",
      //"product-price-savings#saving-pdp",
      // "product-selling-price#pdpsticky"
      "product-price-custom"
    ]
  },
  "vtex.store-components:product-name#sticky": {
    "props": {
      "blockClass": "stickyName",
      "tag": "div"
    }
  },
  "product-image#sticky": {
    "props": {
      "blockClass": "sticky"
    }
  },
  "condition-layout.product#isDiscontinuedSticky": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "condition-layout.binding#checkAccessoriesAndAvailabilityOnBinding"
    }
  },
