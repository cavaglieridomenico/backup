    "props": {
      "blockClass": "logoBrand"
    },
    "children": ["image#logoBrandWhirlpool"]
  },
  "image#logoBrandWhirlpool": {
    "props": {
      "src": "/arquivos/whr-logo.svg",
      "alt": "Whirlpool",
      "maxWidth": 90,
      "blockClass": ["logoBrand", "logoBrandWhirlpool"]
    }
  },
