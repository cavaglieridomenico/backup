  "condition-layout.product#productBrand": {
    "props": {
      "conditions": [
        {
          "subject": "brandId",
          "arguments": {
            "id": "2000000"
          }
        }
      ],
      // "Then": "image#logoBrandWhirlpool",
      "Then": "flex-layout.row#logoBrandWhirlpool",
      "Else": "condition-layout.product#productBrandIndesit"
    }
  },
