  "rich-text#contact-center-desc": {
    "props": {
      "text": "Per richieste di assistenza ed estensioni di garanzia <br> Whirlpool - Hotpoint - Indesit",
      "blockClass": "contact-center-desc"
    }
  },
  "flex-layout.col#support-information-unique-number": {
    "props": {
      "blockClass": "unique-number"
    },
    "children": [
      "rich-text#unique-number-title",
      "link#unique-number-phone",
      "rich-text#unique-number-postilla"
    ]
  },
  "rich-text#unique-number-title": {
    "props": {
      "text": "Numero unico:",
      "blockClass": "unique-number-title"
    }
  },
  "link#unique-number-phone": {
    "props": {
      "label": "02.20.30*",
      "href": "tel:022030",
      "blockClass": "unique-number-phone"
    }
  },
  "rich-text#unique-number-postilla": {
    "props": {
      "text": "*al costo di una chiamata a rete fissa secondo il piano tariffario previsto dal proprio operatore.<br>[Lun-Ven dalle 9:00 alle 18:00]",
      "blockClass": "unique-number-postilla"
    }
  }
}
