  "condition-layout.product#warranty": {
    "title": "[PRODUCT-CONDITION] - Warranty ",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "extendedWarranty",
            "value": "true"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isDiscontinued",
            "value": "false"
          }
        }
      ],
      "Then": "flex-layout.row#banner-warranty-10"
      // "Else": "condition-layout.product#warranty-over"
    }
  },
  "condition-layout.product#warranty-over": {
    "title": "[PRODUCT-CONDITION] - Warranty over",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "extendedWarranty",
            "value": "25"
          }
        }
      ],
      "Then": "flex-layout.row#banner-warranty-25",
      "Else": "condition-layout.product#warranty-over25"
    }
  },
  "condition-layout.product#warranty-over25": {
    "title": "[PRODUCT-CONDITION] - Warranty over 25",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "extendedWarranty",
            "value": "30"
          }
        }
      ],
      "Then": "flex-layout.row#banner-warranty-30"
    }
  },
