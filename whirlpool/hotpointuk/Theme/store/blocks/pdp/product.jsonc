  },
  "condition-layout.product#bannerPromo": {
    "title": "[PRODUCT-CONDITION] - Banner Promo",
    "props": {
      "conditions": [
        {
          "subject": "productClusters",
          "arguments": {
            "id": "156"
          }
        }
      ],
      "Then": "link#promoBannerPromoSmall"
    }
  },
  "condition-layout.product#bannerPromoFairy": {
    "title": "[PRODUCT-CONDITION] - Banner Promo Fairy",
