  "condition-layout.product#isAccessorioIntestazioneTab": {
    "title": "[PRODUCT-CONDITION] - Is Accessories Tab Header",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": { "name": "isDiscontinued", "value": "true" }
        }
      ],
      "matchType": "any",
      "Else": "flex-layout.row#dettagliTitle"
    }
  },
  "condition-layout.product#isAccessorioTab": {
    "title": "[PRODUCT-CONDITION] - Is Accessories",
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        },
        {
