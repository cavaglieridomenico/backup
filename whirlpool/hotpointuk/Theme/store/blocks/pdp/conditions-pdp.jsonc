  //
  "condition-layout.product#pdp-is-spare-accessories": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isSparePart",
            "value": "true"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        }
      ],
      "matchType": "any",
      "Then": "flex-layout.col#fg-product-page-accessories",
      "Else": "flex-layout.col#fg-product-page"
    }
  }
