  "condition-layout.product#isAccessorioseparator": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        }
      ],
      "matchType": "any",
      "Else": "product-specification-group#minimal"
    }
  },
