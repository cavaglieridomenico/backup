          }
        }
      ],
      "matchType": "all",
      "Then": "product-quantity#pdp"
    }
  },
  "condition-layout.product#fg-product-page-is-spare": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "isSparePart",
            "value": "true"
          }
        }
      ],
      "Then": "flex-layout.row#spare-product-page",
      "Else": "flex-layout.row#fg-product-page"
    }
  },
  "condition-layout.product#product-page-is-accessories": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
