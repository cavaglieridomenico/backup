  "condition-layout.product#add-to-cart-is-accessory": {
    "props": {
      "conditions": [
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "constructionType",
            "value": "Accessory"
          }
        },
        {
          "subject": "specificationProperties",
          "arguments": {
            "name": "sellable",
            "value": "false"
          }
        }
      ],
      "matchType": "all",
      "Then": "add-to-cart-custom#plp"
    }
  },
  "condition-layout.product#quantity-is-accessory": {
    "props": {
