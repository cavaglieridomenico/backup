{
  "slider-layout#banner-carousel-top": {
    "props": {
      "itemsPerPage": {
        "desktop": 5,
        "tablet": 3,
        "phone": 1
      },
      "infinite": false,
      "autoplay": {
        "timeout": 3000
      },
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "bannerHeaderContainer"
    },
    "children": [
      "flex-layout.row#prima",
      "flex-layout.row#seconda",
      "flex-layout.row#quinta",
      "flex-layout.row#terza",
      "flex-layout.row#quarta"
    ]
  },

  "flex-layout.row#prima": {
    "props": {
      "blockClass": ["bannerheader", "BorderCenterElements"],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["image#logoBannerHeader-1", "rich-text#BannerHeaderText1"]
  },
  "image#logoBannerHeader-1": {
    "props": {
      "src": "assets/bannerHeader/icon1_small.png",
      "width": 24,
      "height": 24,
      "blockClass": ["iconBelowHeader"]
    }
  },
  "rich-text#BannerHeaderText1": {
    "props": {
      "text": "Consegna gratuita al piano",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": ["bannerheader", "noLinkEffect"]
    }
  },
  "flex-layout.row#seconda": {
    "props": {
      "blockClass": ["bannerheader", "BorderCenterElements"],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["image#logoBannerHeader2", "rich-text#BannerHeaderText2"]
  },
  "image#logoBannerHeader2": {
    "props": {
      "src": "assets/bannerHeader/icon2.svg",
      "width": 20,
      "height": 32,
      "blockClass": ["iconBelowHeader"]
    }
  },
  "rich-text#BannerHeaderText2": {
    "props": {
      "text": "Installazione su richiesta",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "bannerheader"
    }
  },
  "flex-layout.row#quinta": {
    "props": {
      "blockClass": ["bannerheader", "BorderCenterElements"],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["flex-layout.row#imagesBox", "rich-text#BannerHeaderText5"]
  },
  "flex-layout.row#imagesBox": {
    "props": {
      "blockClass": ["bannerHeaderImageBox"]
    },
    "children": ["image#logoBannerHeader5", "image#logoStarsBannerHeader5"]
  },
  "image#logoBannerHeader5": {
    "props": {
      "src": "assets/bannerHeader/icon5.svg",
      "width": 50,
      "height": 12,
      "blockClass": ["iconBelowHeader"]
    }
  },
  "image#logoStarsBannerHeader5": {
    "props": {
      "src": "https://cdn.trustpilot.net/brand-assets/4.1.0/stars/stars-4.5.svg",
      "width": 50,
      "height": 9,
      "blockClass": ["iconBelowHeader", "iconStarsBannerHeader"]
    }
  },
  "rich-text#BannerHeaderText5": {
    "props": {
      "text": "Rated Good <br/> for Service",
      "textAlignment": "LEFT",
      "textPosition": "CENTER",
      "blockClass": ["bannerheader", "noLinkEffect"]
    }
  },
  "flex-layout.row#terza": {
    "props": {
      "blockClass": ["bannerheader", "BorderCenterElements"],
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["image#logoBannerHeader3", "rich-text#BannerHeaderText3"]
  },
  "image#logoBannerHeader3": {
    "props": {
      "src": "assets/bannerHeader/icon3.svg",
      "width": 24,
      "height": 35,
      "blockClass": ["iconBelowHeader"]
    }
  },
  "rich-text#BannerHeaderText3": {
    "props": {
      "text": "Spedizione in 3-5 giorni lavorativi*",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "bannerheader"
    }
  },
  "flex-layout.row#quarta": {
    "props": {
      "blockClass": "bannerheader",
      "preserveLayoutOnMobile": true,
      "fullWidth": true,
      "horizontalAlign": "center",
      "colSizing": "auto"
    },
    "children": ["image#logoBannerHeader4", "rich-text#BannerHeaderText4"]
  },
  "image#logoBannerHeader4": {
    "props": {
      "src": "assets/bannerHeader/icon4.svg",
      "width": 24,
      "height": 36
    }
  },
  "rich-text#BannerHeaderText4": {
    "props": {
      "text": "Se richiesto, ritiro contestuale del tuo usato",
      "textAlignment": "CENTER",
      "textPosition": "CENTER",
      "blockClass": "bannerheader"
    }
  },
  //BAnner Corona
  "slider-layout#banner-carousel-corona": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "autoplay": {
        "timeout": 3000
      },
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "bannerHeaderContainer"
    },
    "children": [
      "flex-layout.row#coronaVirusBanner",
      "flex-layout.row#securityBanner"
    ]
  }
}
