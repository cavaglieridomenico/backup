      "flex-layout.row#newsletter-automatic-popup",
      "flex-layout.row#fullHeaderColABTest"
    ]
  },
  "flex-layout.row#newsletter-automatic-popup": {
    "title": "Row - Newsletter Automatic Popup",
    "children": [
      "newsletter-automatic-popup"
    ],
    "props": {
      "blockClass": "rowNewsletterAutomaticPopup"
    }
  },
  "flex-layout.row#stickyRow": {
    "props": {
      "blockClass": "fullHeaderRow",
      "width": "100%",
      "fullWidth": true
    },
    "children": [
      "sticky-layout#desktop"
    ]
  },
  "flex-layout.row#carousel-topRow": {
    "props": {
      "blockClass": "fullHeaderRow",
      "width": "100%",
      "fullWidth": true
    },
    "children": [
      "slider-layout#banner-carousel-top"
    ]
  },
  "sticky-layout#desktop": {
    "props": {
      "blockClass": "sticky-header"
    },
    "children": [
      "stripe-custom#stripe",
      "flex-layout.row#desktopTop",
      "flex-layout.row#desktopBottom"
    ]
  },
  "stripe-custom#stripe": {
    "title": "Stripe Banner",
    "props": {
      "blockClass": ""
    },
    "children": [
      "rich-text#stripe"
    ]
  },
  "rich-text#stripe": {
    "title": "Testo dello Stripe - Pop Up Banner",
    "props": {
      "blockClass": "stripe-link",
      "text": "**We have recently updated our Privacy Notice. Please review the updated version [here](/privacy-policy).**",
      "textAlignment": "CENTER",
      "textPosition": "CENTER"
    }
  },
  "flex-layout.row#desktopTop": {
    "props": {
      "horizontalAlign": "left",
      "verticalAlign": "center",
      "preventHorizontalStretch": true,
      "preventVerticalStretch": true,
      "fullWidth": true,
      "blockClass": "headerContentTop"
    },
    "children": [
      "flex-layout.col#leftPartHeaderTop",
      "flex-layout.col#rightPartHeaderTop"
    ]
  },
  "flex-layout.col#rightPartHeaderTop": {
    "props": {
      "blockClass": "leftPartHeaderTop"
    },
    "children": [
