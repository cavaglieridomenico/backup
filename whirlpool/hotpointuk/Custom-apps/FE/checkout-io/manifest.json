{
  "vendor": "hotpointuk",
  "name": "checkout-container-custom",
  "version": "0.2.19",
  "title": "Custom Checkout Example",
  "description": "",
  "builders": {
    "assets": "0.x",
    "react": "3.x",
    "store": "0.x",
    "messages": "1.x",
    "node": "6.x",
    "graphql": "1.x",
    "styles": "2.x"
  },
  "dependencies": {
    "vtex.store-graphql": "2.x",
    "vtex.pixel-interfaces": "1.x",
    "vtex.pixel-manager": "1.x",
    "vtex.styleguide": "9.x",
    "vtex.formatted-price": "0.x",
    "vtex.shipping-estimate-translator": "2.x",
    "vtex.flex-layout": "0.x",
    "vtex.rich-text": "0.x",
    "vtex.store-icons": "0.x",
    "vtex.store-components": "3.x",
    "vtex.store-footer": "2.x",
    "vtex.store-header": "2.x",
    "vtex.login": "2.x",
    "vtex.responsive-layout": "0.x",
    "vtex.store": "2.x",
    "vtex.checkout-cart": "0.x",
    "vtex.order-placed": "2.x",
    "vtex.order-manager": "0.x",
    "vtex.checkout-graphql": "0.x",
    "vtex.search-graphql": "0.x",
    "vtex.checkout-resources": "0.x",
    "vtex.render-runtime": "8.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "settingsSchema": {
    "title": "checkout-container-custom",
    "type": "object",
    "properties": {
      "auth": {
        "title": "Auth",
        "type": "object",
        "properties": {
          "appKey": {
            "title": "App key",
            "type": "string"
          },
          "appToken": {
            "title": "App token",
            "type": "string"
          }
        }
      },
      "delivery": {
        "title": "Delivery",
        "type": "object",
        "properties": {
          "paidDeliveryConfig": {
            "title": "Delivery Service Info",
            "type": "object",
            "properties": {
              "deliveryType": {
                "title": "service type",
                "type": "string",
                "default": "Delivery"
              }
            }
          }
        }
      }
    }
  },
  "credentialType": "absolute",
  "policies": [
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*",
        "path": "*"
      }
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "vtexid.vtex.com.br",
        "path": "api/"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "api/checkout/pub/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "*.vtexcommercebeta.com.br",
        "path": "*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercebeta.com.br",
        "path": "*"
      }
    },
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
